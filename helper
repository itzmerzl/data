function SaveToFile(filename, data) Path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\" local File = io.open(Path..filename, "a") File:write(data) File:close() end

function SaveConfiguration()
    Path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\Config.txt"
    File = io.open(Path, "w")
    
    if not File then LogToConsole("`4Error: Unable to create the File on the desktop.") return end

    File:write("\n 'Ability Configurations' \n")
    File:write("Block Super Duper Broadcast = " .. tostring(BlockSDB) .. " \n")
    File:write("Anti Pocket Lighter = " .. tostring(AntiPocket) .. " \n")
    File:write("Auto Pull Gas = " .. tostring(AutoGas) .. " \n")
    File:write("Show Collected Gems = " .. tostring(ShowGems) .. " \n")
    File:write("Extractor = " .. tostring(SelfExtract) .. " \n")
    File:write("Fast Drop = " .. tostring(FastDrop) .. " \n")
    File:write("Fast Trash = " .. tostring(FastTrash) .. " \n")

    File:write("\n 'BTK Configurations' \n")
    File:write("Posx1 = " .. tostring(Posx1) .. ", Posy1 = "..tostring(Posy1).." | Posx2 = " .. tostring(Posx2) .. ", Posy2 = "..tostring(Posy2).." \n")
    File:close()

    LogToConsole("Configuration has been saved to `5'Config.txt'")
end

function LoadConfiguration()
    Path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\Config.txt" File = io.open(Path, "r")
    MainPath = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\scripts\\Helper" MainFile = io.open(MainPath, "a")
    
    if not File then LogToConsole("`4Error: Unable to create the File on the desktop.") return end

    for line in File:lines() do
    MainFile:write(line)
    end
    MainFile:close() File:close()

    LogToConsole("Configuration has been loaded")
end

if Command:find("/load") then LoadConfiguration() return true end
if Command:find("/save") then SaveConfiguration() return true end

if Command:find("/saveworld") then local args = Divider(Command," ") if (args[2] == nil) or (args[2] == "") or (args[2] == " ") then if SaveWorld == nil or SaveWorld == " " or SaveWorld == "" then LogToConsole("Save world is `4empty`o, type command `5/saveworld <world> `oto save world") else LogToConsole("Warping to save world : `5" .. SaveWorld) Sleep(200) SendPacket(3, ("action|join_request\nname|%s\ninvitedWorld|0"):format(SaveWorld)) Sleep(300)  end else SaveWorld = args[2]:upper() LogToConsole("Save world set to : `5" .. SaveWorld) end return true end 
if Command:find("/ping") then if GetPing() > 150 then Color = "`9" end if GetPing() > 400 then Color = "`4" end if GetPing() < 150 then Color = "`2" end LogToConsole("Your ping latency : "..Color.. tostring(GetPing()):gsub(".0", "")) return true end 

function BuySurgicalKit() for i = 1, 50 do SendPacket(2,"action|buy\nitem|buy_surgkit") Sleep(180) end LogToConsole("Buying 250 Surgical Pack succesful!") BuyingSurgKit = false if DropSurgKit then DroppingSurgKit = true end end
function BuyCookingPack() for i = 1, 10 do SendPacket(2,"action|buy\nitem|buy_cookingpack") Sleep(180) end LogToConsole("Buying Cooking Pack succesful!") BuyingCookPack = false if DropCookPack then DroppingCookPack = true end end

function DropSurgicalKit() Sleep(150) Drop(1240, GetAmount(1240)) Sleep(150) Drop(1256, GetAmount(1256)) Sleep(150) Drop(1258, GetAmount(1258)) Sleep(150) Drop(1260, GetAmount(1260)) Sleep(150) Drop(1262, GetAmount(1262)) Sleep(150) Drop(1264, GetAmount(1264)) Sleep(150) Drop(1266, GetAmount(1266)) Sleep(150) Drop(1268, GetAmount(1268)) Sleep(150) Drop(1270, GetAmount(1270)) Sleep(150) LogToConsole("Dropped all surgical tools") end
function DropCookingPack() ItemCook = { 4568, 4570, 4572, 956, 4562, 962, 4602, 4588, 4578, 4586, 874, 868, 4766, 4672, 4666, 822, 4582, 4618 } for _, item in pairs(ItemCook) do Drop(item, GetAmount(item)) Sleep(180) end LogToConsole("Dropped all surgical tools") end

