end
if var[0] == "OnSpawn" and var[1] == "invis|1" then Warn("`#@Guardian `4Just joined the world.") Bubble("Invisible `#@Moderator `4Just joined the world.") return true end
end

if var[0]:find("OnSpawn") then
local args = Divider(var[1],"\n") LastName = FormatName(string.sub(args[6], 6,40))
--table.insert(ActivityLog, {enter = "\nadd_label_with_icon|small|"..string.sub(args[6], 6,40).." `oentered `2"..GetWorld().name.." `oat "..os.date("%H:%M on %d/%m").."|left|1400|\n", netid = GetLocal().netid})
