function Dialog_WorldToWorld() if ItemToMove == 0 then FloatItem = "`4None" else FloatItem = GetItemInfo(math.floor(ItemToMove)).name end if ActionFrom == "Float" then frameFromFloat = "staticPurpleFrame" else frameFromFloat = "staticGreyFrame" end if ActionFrom == "Magplant" then frameFromMag = "staticPurpleFrame" else frameFromMag = "staticGreyFrame" end if ActionFrom == "Vending" then frameFromVend = "staticPurpleFrame" else frameFromVend = "staticGreyFrame" end if ActionTo == "Float" then frameToFloat = "staticPurpleFrame" else frameToFloat = "staticGreyFrame" end if ActionTo == "Magplant" then frameToMag = "staticPurpleFrame" else frameToMag = "staticGreyFrame" end if ActionTo == "Vending" then frameToVend = "staticPurpleFrame" else frameToVend = "staticGreyFrame" end W = [[
