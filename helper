if Command:find("/posrbet") then if not SetupPosBetReme then SetupPosBetReme = true Bubble("Punch display box for setup the position") else SetupPosBetReme = false Bubble("Setup position bet closed!") end return true end

if Command:find("/blink") or Command:find("/skinblink") then if SkinBlink then SkinBlink = false LogToConsole("Skin blink `4Disabled") return true else SkinBlink = true LogToConsole("Skin blink `2Enabled") end return true end 

if Command:find("/spin ") then local args = Divider(Command," ") SpunNumber = tonumber(args[2]) RandomFakeColor = {"`b", "`4"} if SpunNumber == 0 then Chat("/me ````````````````spun the wheel and got `2"..SpunNumber.."``!]``") else Chat("`w[``"..FormatName(GetLocal().name).."```````````````` spun the wheel and got "..RandomFakeColor[math.random(1, #RandomFakeColor)]..""..SpunNumber.."``!]``") end return true end

if Command:find("/arroz") then local args = Divider(Command," ") Amount = tonumber(args[2]) if Amount == 0 then LogToConsole("You can't drop arroz with 0 amount (lol)") return true elseif GetAmount(4604) < Amount then LogToConsole("You don't have that amount!") return true else DropArroz = true end return true end
if Command:find("/clover") then local args = Divider(Command," ") Amount = tonumber(args[2]) if Amount == 0 then LogToConsole("You can't drop clover with 0 amount (lol)") return true elseif GetAmount(528) < Amount then LogToConsole("You don't have that amount!") return true else DropClover = true end return true end
if Command:find("/anc") then local args = Divider(Command," ") Amount = tonumber(args[2]) if Amount == 0 then LogToConsole("You can't drop arroz and clover with 0 amount (lol)") return true elseif GetAmount(4604) < Amount or GetAmount(528) < Amount then LogToConsole("You don't have that amount!") return true else DropAnC = true end return true end

if Command:find("/wl") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true elseif GetAmount(242) < tonumber(args[2]) then LogToConsole(Oops.."You didn't have that amount to drop!") return true else AmountD = tonumber(args[2]) DropWlOnly = true LogToConsole("Dropped `w"..tonumber(args[2]).." `9World Lock") if DropBubble then Chat(Credit.."`wDropped "..tonumber(args[2]).." `9World Lock") end end return true end 
if Command:find("/dwl") then if GetAmount(242) <= 0 then LogToConsole(Oops.."You didn't have `9World Lock `oto drop!") return true else DropAllWl = true LogToConsole("Dropped `w"..GetAmount(242).." `9World Lock") if DropBubble then Chat(Credit.."`wDropped "..GetAmount(242).." `9World Lock") end end return true end 

if Command:find("/dl") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true elseif GetAmount(1796) < tonumber(args[2]) then LogToConsole(Oops.."You didn't have that amount to drop!") return true else AmountD = tonumber(args[2]) DropDlOnly = true LogToConsole("Dropped `w"..tonumber(args[2]).." `1Diamond Lock") if DropBubble then Chat(Credit.."`wDropped "..tonumber(args[2]).." `1Diamond Lock") end end return true end 
if Command:find("/ddl") then if GetAmount(1796) <= 0 then LogToConsole(Oops.."You didn't have `1Diamond Lock `oto drop!") return true else DropAllDl = true LogToConsole("Dropped `w"..GetAmount(1796).." `1Diamond Lock") if DropBubble then Chat(Credit.."`wDropped "..GetAmount(1796).." `1Diamond Lock") end end return true end 

if Command:find("/bl") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true elseif GetAmount(7188) < tonumber(args[2]) then LogToConsole(Oops.."You didn't have that amount to drop!") return true else AmountD = tonumber(args[2]) DropBglOnly = true LogToConsole("Dropped `w"..tonumber(args[2]).." `eBlue Gem Lock") if DropBubble then Chat(Credit.."`wDropped "..tonumber(args[2]).." `eBlue Gem Lock") end end return true end 
if Command:find("/dbl") then if GetAmount(7188) <= 0 then LogToConsole(Oops.."You didn't have `eBlue Gem Lock `oto drop!") return true else DropAllBgl = true LogToConsole("Dropped `w"..GetAmount(7188).." `eBlue Gem Lock") if DropBubble then Chat(Credit.."`wDropped "..GetAmount(7188).." `eBlue Gem Lock") end end return true end 

if Command:find("/bg") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true elseif GetAmount(11550) < tonumber(args[2]) then LogToConsole(Oops.."You didn't have that amount to drop!") return true else AmountD = tonumber(args[2]) DropIrengOnly = true LogToConsole("Dropped `w"..tonumber(args[2]).." `bBlack Gem Lock") if DropBubble then Chat(Credit.."`wDropped "..tonumber(args[2]).." `bBlack Gem Lock") end end return true end 
if Command:find("/dbg") then if GetAmount(11550) <= 0 then LogToConsole(Oops.."You didn't have `bBlack Gem Lock `oto drop!") return true else DropAllIreng = true LogToConsole("Dropped `w"..GetAmount(11550).." `bBlack Gem Lock") if DropBubble then Chat(Credit.."`wDropped "..GetAmount(11550).." `bBlack Gem Lock") end end return true end 

if Command:find("/daw") then WL, DL, BGL, IRENG = GetAmount(242), GetAmount(1796), GetAmount(7188), GetAmount(11550) Amount = WL+(DL*100)+(BGL*10000)+(IRENG*1000000) Daw = true return true end 

if Command:find("/cdrop") or Command:find("/cd") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true else Amount = tonumber(args[2]) CustomDrop = true end return true end 
if Command:find("/ddrop") or Command:find("/dd") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true else Amount = tonumber(args[2] * 100) CustomDrop = true end return true end 
if Command:find("/bdrop") or Command:find("/bd") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true else Amount = tonumber(args[2] * 10000) CustomDrop = true end return true end 
if Command:find("/bgdrop") or Command:find("/bgd") then local args = Divider(Command," ") if tonumber(args[2]) == nil then LogToConsole(AmountNilText) return true elseif tonumber(args[2]) == 0 then LogToConsole(Amount0Text) return true else Amount = tonumber(args[2] * 1000000) CustomDrop = true end return true end 

