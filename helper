elseif var[1]:find("CP:") then Name = string.sub(var[1], var[1]:find("6<`") + 4, var[1]:find("`>")) LogToConsole("`7[`4FAKE`7] "..var[1]) Warn("`wWarning from `4Starries Helper `w: Someone typing in your world!") Bubble(Name.." `wcaught using `4FAKE `wwheel!") SendVariantList({[0] = "OnTalkBubble", [1] = GettingNetid(Name), [2] = "`7[`4FAKE`7]" .. string.sub(var[1], var[1]:find(">") + 1, var[1]:find("!]") + 1 )}) end 
