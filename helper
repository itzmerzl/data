if Command:find("/td") or Command:find("/tdrop") then if Bet == 0 then LogToConsole("Set amount bet with command `5/bet <amount>")return true end Amount = Bet * 2 - CountTax(Bet * 2, Tax1) DropOnlyTax = true return true end 
if Command:find("/gd") or Command:find("/gdrop") then if Bet == 0 then LogToConsole("Set amount bet with command `5/bet <amount>") return true end Amount = CountTax(Bet * 2, Tax1) DropOnlyBet = true  return true end 

if Command:find("/split") then local args = Divider(Command," ") Percentage = tonumber(args[2]) local Count = 0 for _, item in pairs(GetInventory()) do if (item.id == 242) then Count = Count + item.amount end if (item.id == 1796) then Count = Count+ item.amount * 100 end if (item.id == 7188) then Count = Count + item.amount * 10000 end if (item.id == 11550) then Count = Count + item.amount * 1000000 end end Amount = CountTax(Count, Percentage) DropSplit = true return true end 
