if var[0] == "OnStorePurchaseResult" and ChangePurchaseItem then if var[1]:find("You've purchased `o(.+) ``for") then PurchasingItem = var[1]:match("You've purchased `o(.+) ``for") Overlay(Credit.."`wPurchasing Item set to : "..PurchasingItem) LogToConsole("Purchasing Item set to : "..PurchasingItem) ChangePurchaseItem = false end end

return false end)

AddHook("OnSendPacketRaw","PacketRaw",function(pkt)

if NoPickup then if pkt.type == 11 then for _, object in pairs(GetObjectList()) do if object.oid == pkt.value and not NoPickupTable[math.floor(object.id)] then return true elseif object.oid == pkt.value and NoPickupTable[math.floor(object.id)] then return true end end end end
if ShowGems then if pkt.type == 11 then for _, object in pairs(GetObjectList()) do if object.oid == pkt.value and object.id == 112 then Gems = Gems + object.amount Bubble("`wCollected `5"..Gems.." `wGems (gems)") end end end end

if (pkt.type == 3 and pkt.value == 18) then

if PunchConfig then
for _, tile in pairs(GetTiles()) do 
if tile.fg == 1422 or tile.fg == 2488 then if tile.x == pkt.px and tile.y == pkt.py then if LeftDisplay and not RightDisplay then Posx1 = math.floor(pkt.px) Posy1 = math.floor(pkt.py) LogToConsole("`7[`6POS`7] `oPlayer 1 Position saved! `7[`5" .. Posx1.. "`w,`5" .. Posy1.."`7]") Bubble("Next, Punch display box for position 2") LeftDisplay = false RightDisplay = true elseif RightDisplay and not LeftDisplay then Posx2 = math.floor(pkt.px) Posy2 = math.floor(pkt.py) LogToConsole("`7[`6POS`7] `oPlayer 2 Position saved! `7[`5" .. Posx2.. "`w,`5" .. Posy2.."`7]") RightDisplay = false PunchConfig = false Bubble("Succesful setting position bet!") if FastSettingPos then HosterConfig = true Bubble("Next, Punch position for hoster") end end end end
if tile.fg == 1452 or tile.fg == 10784 then if tile.x == pkt.px and tile.y == pkt.py then if DonationBox and not LeftDisplay and not RightDisplay then Postx = math.floor(pkt.px) Posty = math.floor(pkt.py) LogToConsole("`7[`6POS`7] `oPosition donation box saved! `7[`5" .. Postx.. "`w,`5" .. Posty.."`7]") DonationBox = false PunchConfig = false end end end
if tile.fg == 5638 then if tile.x == pkt.px and tile.y == pkt.py then if FinderMagplant and not DonationBox and not LeftDisplay and not RightDisplay then FindMagx = math.floor(pkt.px) FindMagy = math.floor(pkt.py) LogToConsole(Fi.."`oMagplant Position saved! `7[`5" ..FindMagx.. "`w,`5" ..FindMagy.."`7]") FinderMagplant = false PunchConfig = false end end end
if tile.fg == 9268 or tile.fg == 2978 then if tile.x == pkt.px and tile.y == pkt.py then if FinderVending and not FinderMagplant and not DonationBox and not LeftDisplay and not RightDisplay then FindVendx = math.floor(pkt.px) FindVendy = math.floor(pkt.py) LogToConsole(Fi.."`oVending Position saved! `7[`5" ..FindVendx.. "`w,`5" ..FindVendy.."`7]") FinderVending = false PunchConfig = false end end end
end return true
end

if HosterConfig then for _, tile in pairs(GetTiles()) do if tile.x == pkt.px and tile.y == pkt.py then Hostx, Hosty = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`6POS`7] `oPosition Hoster saved! [`5" .. Hostx.. "`7,`5" .. Hosty.."`7]") HosterConfig = false if FastSettingPos then FastSettingPos = false Dialog_PosChand() end end end return true end

if PunchFromFloat then for _, tile in pairs(GetTiles()) do if tile.x == pkt.px and tile.y == pkt.py then WtwFloatx1, WtwFloaty1 = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`eWTW`7]`o Floating 1 Position saved! `7[`5" .. WtwFloatx1.. "`w,`5" .. WtwFloaty1.."`7]") PunchFromFloat = false end end end
if PunchFromMag then for _, tile in pairs(GetTiles()) do if tile.fg == 5638 and tile.x == pkt.px and tile.y == pkt.py then WtwMagx1, WtwMagy1 = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`eWTW`7]`o Magplant 1 Position saved! `7[`5" .. WtwMagx1.. "`w,`5" .. WtwMagy1.."`7]") PunchFromMag = false end end end
if PunchFromVend then for _, tile in pairs(GetTiles()) do if (tile.fg == 9268 or tile.fg == 2978) and tile.x == pkt.px and tile.y == pkt.py then WtwVendx1, WtwVendy1 = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`eWTW`7]`o Vending 1 Position saved! `7[`5" .. WtwVendx1.. "`w,`5" .. WtwVendy1.."`7]") PunchFromVend = false end end end
if PunchToFloat then for _, tile in pairs(GetTiles()) do if tile.x == pkt.px and tile.y == pkt.py then WtwFloatx2, WtwFloaty2 = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`eWTW`7]`o Floating 2 Position saved! `7[`5" .. WtwFloatx2.. "`w,`5" .. WtwFloaty2.."`7]") PunchToFloat = false end end end
if PunchToMag then for _, tile in pairs(GetTiles()) do if tile.fg == 5638 and tile.x == pkt.px and tile.y == pkt.py then WtwMagx2, WtwMagy2 = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`eWTW`7]`o Magplant 2 Position saved! `7[`5" .. WtwMagx2.. "`w,`5" .. WtwMagy2.."`7]") PunchToMag = false end end end
if PunchToVend then for _, tile in pairs(GetTiles()) do if (tile.fg == 9268 or tile.fg == 2978) and tile.x == pkt.px and tile.y == pkt.py then WtwVendx2, WtwVendy2 = math.floor(pkt.px), math.floor(pkt.py) LogToConsole("`7[`eWTW`7]`o Vending 2 Position saved! `7[`5" .. WtwVendx2.. "`w,`5" .. WtwVendy2.."`7]") PunchToVend = false end end end

if SetupPosBetReme then PosReme = {}
for _, tile in pairs(GetTiles()) do 
if tile.fg == 1422 or tile.fg == 2488 then 
if tile.x == pkt.px and tile.y == pkt.py then 
table.insert(PosReme, "x = "..math.floor(pkt.px).." y = "..math.floor(pkt.py)) for key,value in ipairs(PosReme) do print(key,value) end Position = "" LogToConsole("`7[`6POS`7] `oPlayer Bet Position saved! `7[`5"..table.concat(PosReme).."`7]") break 
if not SetupPosBetReme then break end
end 
end
end
end

end

if (pkt.type == 3 and pkt.value == 18 or pkt.value == 340) then

if PlaceConfig then
for _, tile in pairs(GetTiles()) do
if tile.x == pkt.px and tile.y == pkt.py then
if Horizontal then Vertical = false if PosChand1 and not PosChand2 and not LeftDisplay and not RightDisplay then Posxgem1 = math.floor(pkt.px) Posygem1 = math.floor(pkt.py) Posxgem2 = Posxgem1 + 1 Posxgem3 = Posxgem1 - 1 Tiles = {Pos1 = {{x = Posxgem2, y = Posygem1}, {x = Posxgem1, y = Posygem1}, {x = Posxgem3, y = Posygem1}},Pos2 = {{x = PPosxgem2, y = PPosygem1}, {x = PPosxgem1, y = PPosygem1}, {x = PPosxgem3, y = PPosygem1}},} PosChand1 = false PosChand2 = true LogToConsole("`7[`6POS`7] `oPosition Chand saved! [ `5" ..Posxgem2.. "`o, `5" ..Posygem1.. " `oI `5" ..Posxgem1.. "`o, `5" ..Posygem1.. " `oI `5" ..Posxgem3.. "`o, `5" ..Posygem1.." `o]") elseif PosChand2 and not PosChand1 then PPosxgem1 = math.floor(pkt.px) PPosygem1 = math.floor(pkt.py) PPosxgem2 = PPosxgem1 + 1 PPosxgem3 = PPosxgem1 - 1 Tiles = {Pos1 = {{x = Posxgem2, y = Posygem1}, {x = Posxgem1, y = Posygem1}, {x = Posxgem3, y = Posygem1}},Pos2 = {{x = PPosxgem2, y = PPosygem1}, {x = PPosxgem1, y = PPosygem1}, {x = PPosxgem3, y = PPosygem1}},} PosChand2 = false PlaceConfig = false LogToConsole("`7[`6POS`7] `oPosition Chand saved! [ `5" ..PPosxgem2.. "`o, `5" ..PPosygem1.. " `oI `5" ..PPosxgem1.. "`o, `5" ..PPosygem1.. " `oI `5" ..PPosxgem3.. "`o, `5" ..PPosygem1.." `o]") Bubble("Succesful setting position chand!") end end
if Vertical then Horizontal = false if PosChand1 and not PosChand2 and not LeftDisplay and not RightDisplay then Posxgem1 = math.floor(pkt.px) Posygem1 = math.floor(pkt.py) Posygem2 = Posygem1 - 1 Posygem3 = Posygem1 - 2 Tiles = {Pos1 = {{x = Posxgem1, y = Posygem1}, {x = Posxgem1, y = Posygem2}, {x = Posxgem1, y = Posygem3}},Pos2 = {{x = PPosxgem1, y = PPosygem1}, {x = PPosxgem1, y = PPosygem2}, {x = PPosxgem1, y = PPosygem3}},} PosChand1 = false PosChand2 = true LogToConsole("`7[`6POS`7] `oPosition Chand saved! [ `5" ..Posxgem1.. "`o, `5" ..Posygem1.. " `oI `5" ..Posxgem1.. "`o, `5" ..Posygem2.. " `oI `5" ..Posxgem1.. "`o, `5" ..Posygem3.." `o]") elseif PosChand2 and not PosChand1 then PPosxgem1 = math.floor(pkt.px) PPosygem1 = math.floor(pkt.py) PPosygem2 = PPosygem1 - 1 PPosygem3 = PPosygem1 - 2 Tiles = {Pos1 = {{x = Posxgem1, y = Posygem1}, {x = Posxgem1, y = Posygem2}, {x = Posxgem1, y = Posygem3}},Pos2 = {{x = PPosxgem1, y = PPosygem1}, {x = PPosxgem1, y = PPosygem2}, {x = PPosxgem1, y = PPosygem3}},} PosChand2 = false PlaceConfig = false LogToConsole("`7[`6POS`7] `oPosition Chand saved! [ `5" ..PPosxgem1.. "`o, `5" ..PPosygem1.. " `oI `5" ..PPosxgem1.. "`o, `5" ..PPosygem2.. " `oI `5" ..PPosxgem1.. "`o, `5" ..PPosygem3.." `o]") Bubble("Succesful setting position chand!") end end
end
end
end

end

return false end)

while true do Sleep(150)
if AutoSpam then Sleep(50) StartSpam() end
if CustomDrop then Sleep(50) Cdrop() CustomDrop = false Amount = 0 end
if TakingBet then Sleep(50) TakeBet() TakingBet = false end
if ScanningGems then Sleep(50) ScanGems() ScanningGems = false end
if CollectingGems then Sleep(50) CollectGems() CollectingGems = false if CollectGemsWin then if Winner > 0 then SetWinner(Winner) else LogToConsole("There's no winner, the game ends in a draw!") end end end
if AutoTax then Sleep(150) Cdrop() AutoTax = false Amount = 0 end
if ResetChand1 and Horizontal then Sleep(50) Bubble("Do not move while put chand") ChangeValue("[C] Modfly", true) Sleep(250) FindPath(Posxgem1, Posygem1) Sleep(150) Place(PutChandID, Posxgem1, Posygem1) Sleep(150) Place(PutChandID, Posxgem2, Posygem1) Sleep(150) Place(PutChandID, Posxgem3, Posygem1) Sleep(150) CollectChandPos1() Sleep(150) FindPath(PPosxgem1, PPosygem1) Sleep(150) Place(PutChandID, PPosxgem1, PPosygem1) Sleep(150) Place(PutChandID, PPosxgem2, PPosygem1) Sleep(150) Place(PutChandID, PPosxgem3, PPosygem1) Sleep(150) CollectChandPos2() FindPath(Hostx, Hosty) ChangeValue("[C] Modfly", false) ResetChand1 = false if not PutChand then Chat(AfterDropGameBubbleText) else Bubble("Placing chand with "..PutMode.." `2succesful") end PutChand = false Bet = 0 end
if ResetChand2 and Vertical then Sleep(50) Bubble("Do not move while put chand") ChangeValue("[C] Modfly", true) Sleep(250) FindPath(Posxgem1, Posygem2) Sleep(150) Place(PutChandID, Posxgem1, Posygem1) Sleep(150) Place(PutChandID, Posxgem1, Posygem2) Sleep(150) Place(PutChandID, Posxgem1, Posygem3) Sleep(150) CollectChandPos1() Sleep(150) FindPath(PPosxgem1, PPosygem2) Sleep(150) Place(PutChandID, PPosxgem1, PPosygem1) Sleep(150) Place(PutChandID, PPosxgem1, PPosygem2) Sleep(150) Place(PutChandID, PPosxgem1, PPosygem3) Sleep(150) CollectChandPos2() FindPath(Hostx, Hosty) ChangeValue("[C] Modfly", false) ResetChand2 = false if not PutChand then Chat(AfterDropGameBubbleText) else Bubble("Placing chand with "..PutMode.." `2succesful") end PutChand = false Bet = 0 end
if BackBetNow then Sleep(100) BackBet() BackBetNow = false Amount = 0 end
if DropOnlyTax then Sleep(50) Cdrop() Amount = 0 DropOnlyTax = false end
if DropOnlyBet then Sleep(50) Cdrop() Amount = 0 DropOnlyBet = false end
if DropSplit then Sleep(50) Cdrop() Amount = 0 DropSplit = false end
if GiveTheTax then Sleep(50) LogToConsole("Working...") Sleep(100) Place(32, Postx, Posty) Sleep(100) SendPacket(2,"action|dialog_return\ndialog_name|donate_edit\nx|"..Postx.."|\ny|"..Posty.."|\ndonsel|242") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_item\nx|"..Postx.."|\ny|"..Posty.."|\nitem|242|\ndoncount|"..SplittedWL.."\noptNote|`7[`5Starries`7] `wAuto B2 Tax") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_edit\nx|"..Postx.."|\ny|"..Posty.."|\ndonsel|1796") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_item\nx|"..Postx.."|\ny|"..Posty.."|\nitem|1796|\ndoncount|"..SplittedDL.."\noptNote|`7[`5Starries`7] `wAuto B2 Tax") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_edit\nx|"..Postx.."|\ny|"..Posty.."|\ndonsel|7188") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_item\nx|"..Postx.."|\ny|"..Posty.."|\nitem|7188|\ndoncount|"..SplittedBGL.."\noptNote|`7[`5Starries`7] `wAuto B2 Tax") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_edit\nx|"..Postx.."|\ny|"..Posty.."|\ndonsel|11550") Sleep(200) SendPacket(2,"action|dialog_return\ndialog_name|donate_item\nx|"..Postx.."|\ny|"..Posty.."|\nitem|11550|\ndoncount|"..SplittedIRENG.."\noptNote|`7[`5Starries`7] `wAuto B2 Tax") TaxDonated = Config((SplittedWL)+(SplittedDL*100)+(SplittedBGL*10000)+(SplittedIRENG*1000000)) LogToConsole("`6>> This tax : "..TaxDonated.."`o, has been paid!") Chat(TaxDonated.."`w, has been paid!") GiveTheTax = false end
if Daw then Sleep(50) Cdrop() Daw = false Amount = 0 end
if CVBGL then Sleep(50) ChangeBGL() CVBGL = false end
if BUYDL then Sleep(50) BuyingDL() BUYDL = false end
if DropBlackGems then Sleep(50) DropBgems() end
if PurchasingFromStore then Sleep(50) for i = 1, MassBuy do SendPacket(2,"action|buy\nitem|"..StoreCode) Sleep(200) end LogToConsole("Buying `5"..PurchasingItem.." `oin total : `2"..MassBuy.." `osuccesful!") PurchasingFromStore = false end
if PurchasingFromHub then Sleep(50) for i = 1, AmountHubBuy do SendPacket(2,"action|dialog_return\ndialog_name|activity_purchase\noffer|"..HubCode.."|") Sleep(200) end LogToConsole("Buying `5"..HubItem.." `oin total : `2"..AmountHubBuy.." `osuccesful!") PurchasingFromHub = false end
if Relog then Sleep(50) SendPacket(3,"action|quit_to_exit") Sleep(100) SendPacket(3, "action|join_request\n|name|"..LatestWorld.."\n|invitedWorld|0") Relog = false end
if CheckingBalance then Sleep(50) SendPacket(2,"action|dialog_return\ndialog_name|popup\nbuttonClicked|bgls") Sleep(500) Chat("/kit") Sleep(500) SendPacket(2,"action|wrench\n|netid|"..GetLocal().netid) Sleep(1000) CheckingBalance = false end
if Presence then Sleep(50) Chat("Absen Dulu Adik-Adik (lol)") Sleep(1000) for _, player in pairs(GetPlayerList()) do if player.userid ~= GetLocal().userid and not player.name:find("Slave") then Chat("Hi "..player.name) Sleep(1000) end end LogToConsole("There are no other players beside you") Chat("Absen Selesai (halo)") Presence = false end
if MessageALL then Sleep(50) for _, player in pairs(GetPlayerList()) do if player.userid ~= GetLocal().userid then Chat("/msg "..FormatName(player.name).." "..AutoMessage) Sleep(1000) end end LogToConsole("Message to all player done!") MessageALL = false end
if TradeALL then Sleep(50) for _, player in pairs(GetPlayerList()) do if player.userid ~= GetLocal().userid then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..player.netid.."|\nbuttonClicked|trade") Sleep(50) SendPacket(2,"action|trade_cancel") Sleep(400) end end LogToConsole("Trading all player done!") TradeALL = false end
if PullALL then Sleep(50) for _, player in pairs(GetPlayerList()) do if player.userid ~= GetLocal().userid then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..player.netid.."|\nbuttonClicked|pull") Sleep(400) end end LogToConsole("Pulling all player done!") PullALL = false end
if KickALL then Sleep(50) for _, player in pairs(GetPlayerList()) do if player.userid ~= GetLocal().userid then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..player.netid.."|\nbuttonClicked|kick") Sleep(400) end end LogToConsole("Kicking all player done!") KickALL = false end
if BanALL then Sleep(50) for _, player in pairs(GetPlayerList()) do if player.userid ~= GetLocal().userid then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..player.netid.."|\nbuttonClicked|world_ban") Sleep(400) end end LogToConsole("Banning all player done!") BanALL = false end
if EmojisChat then Sleep(50) Chat("(agree) (alien) (bheart) (build) (bunny) (cactus) (cake) (clap) (cool) (cry) (dance) (eyes) (evil) (fireworks)") Sleep(1000) Chat("(football) (gems) (ghost) (gift) (grin) (grow) (gtoken) (halo) (heart) (heartarrow) (ill) (kiss) (lol) (love) (lucky)") Sleep(1000) Chat("(mad) (megaphone) (music) (moyai) (no) (nuke) (oops) (party) (peace) (pine) (pizza) (plead) (punch) (see-no-evil)") Sleep(1000) Chat("(shamrock) (shy) (sigh) (sleep) (smile) (song) (terror) (tongue) (troll) (turkey) (vend) (weary) (wink) (wl) (wow) (yes)") EmojisChat = false end
if CryingChat then Sleep(50) Chat("(cry)") CryingChat = false end
if PromosiDulu then Sleep(50) Chat("`5Starries Helper `whas several interesting features (wow),") Sleep(1000) Chat("`wfor example : btk helper, sb helper, world finder, clear water/float, many more (nuke)!") PromosiDulu = false end
if CollectRange then Sleep(50) CollectInRange() CollectRange = false end
if CollectCRange then Sleep(50) Overlay("`wCollecting in `2"..CustomRange.." `wRange") CollectInCRange() CustomRange = 0 CollectCRange = false end
if GrowscanBlock then Sleep(50) ScanBlocks() GrowscanBlock = false end
if GrowscanFloat then Sleep(50) ScanFloats() GrowscanFloat = false end
if SkinBlink then Sleep(50) SkinnyBlink() end
if ShowModal then Sleep(50) Res = (GetAmount(242))+(GetAmount(1796)*100)+(GetAmount(7188)*10000)+(GetAmount(115500)*1000000) Chat("I have "..Config(Res)) ShowModal = false end
if ClearingWater then Sleep(50) ChangeValue("[C] Modfly", true) ClearWater() end
if GettingFloat then Sleep(50) ChangeValue("[C] Modfly", true) GetFloating() end
if BreakPlatform then Sleep(50) ChangeValue("[C] Modfly", true) BreakingPlatform() end
if BuildPlatform then Sleep(50) ChangeValue("[C] Modfly", true) BuildingPlatform() end
if FindWorld then Sleep(50) FindWorldinAction() end
if DonasiBuatStarries then Sleep(50) DonationInAction() Amount = 0 DonasiBuatStarries = false end
if SendSupportMessage then Sleep(50) SupportInAction() SendSupportMessage = false end
if BuyingSurgKit then Sleep(50) BuySurgicalKit() end
if BuyingCookPack then Sleep(50) BuyCookingPack() end
if DroppingSurgKit then Sleep(50) DropSurgicalKit() DroppingSurgKit = false end
if DroppingCookPack then Sleep(50) DropCookingPack() DroppingCookPack = false end
if ConsumeNow then Sleep(50) Consumeable() ConsumeNow = false end
if TestWebhookSB then Sleep(50) if MentionUser then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, "Broadcast timer will appear here", "Broadcast! eh just kidding, this is just a webhook test", "Broadcast text will appear here")) TestWebhookSB = false end
if StartingSuperBroadcast then Sleep(50) Broadcasting() end
if ItemFinder then Sleep(50) FinderInAction() end
if WtwRunning then Sleep(50) WorldToWorldInAction() end
if HostingBTK then Sleep(50) BtkAction() end
if TeleportToPos1 then Sleep(50) FindPath(Posx1, Posy1) Bubble("Teleported to pos 1") TeleportToPos1 = false end
if TeleportToPos2 then Sleep(50) FindPath(Posx2, Posy2) Bubble("Teleported to pos 2") TeleportToPos2 = false end
if TeleportToPosH then Sleep(50) FindPath(Hostx, Hosty) Bubble("Teleported to pos hoster") TeleportToPosH = false end
if ShowColekted then Sleep(200) ShowColek() end
if ShowCollected then Sleep(50) Res = Config((CollectWL)+((CollectDL)*100)+((CollectBGL)*10000)+((CollectIRENG)*1000000)) Chat(Credit.."`wCollected "..Res) ShowCollected = false end
if DropWlOnly then Sleep(50) Drop(242, AmountD) DropWlOnly = false AmountD = 0 end if DropDlOnly then Sleep(50) Drop(1796, AmountD) DropDlOnly = false AmountD = 0 end if DropBglOnly then Sleep(50) Drop(7188, AmountD) DropBglOnly = false AmountD = 0 end if DropIrengOnly then Sleep(50) Drop(11550, AmountD) DropIrengOnly = false AmountD = 0 end
if DropAllWl then Sleep(50) Drop(242, GetAmount(242)) DropAllWl = false end if DropAllDl then Sleep(50) Drop(1796, GetAmount(1796)) DropAllDl = false end if DropAllBgl then Sleep(50) Drop(7188, GetAmount(7188)) DropAllBgl = false end if DropAllIreng then Sleep(50) Drop(11550, GetAmount(11550)) DropAllIreng = false end
if ShowWorldGems then Sleep(50) Chat(Credit.."`wThis world has `5"..FloatingAmount(112).." `wGems (gems)") ShowWorldGems = false end
if ShowWorldPlayer then Sleep(50) Chat(Credit.."`wThis world has `5"..GetPlayerCount().." `wPlayers (wow)") ShowWorldPlayer = false end
if RunJutsu then Sleep(50) CurWorld = GetWorld().name Chat("/love") Sleep(800) Chat("/wave") Sleep(800) Chat("`9Minato `w: Hirashin no Jutsu") Sleep(1000) RequestJoinWorld("EXIT") Sleep(1000) RequestJoinWorld(CurWorld) Sleep(1000) Chat("(lol)") RunJutsu = false end
if GoToCarnival then Sleep(50) RequestJoinWorld("CARNIVAL") Sleep(5000) FindPath(52, 14, 150) Sleep(500) Chat(Credit.."`wYou have arrived at carnival") Sleep(500) Place(32, math.floor(GetLocal().pos.x/32), math.floor(GetLocal().pos.y/32)) GoToCarnival = false end
if DropArroz then Sleep(50) Drop(4604, Amount) LogToConsole("Dropped `w"..Amount.." Arroz Con Pollo") if DropBubble then Chat(Credit.."`wDropped `6"..Amount.." `wArroz Con Pollo") end DropArroz = false end
if DropClover then Sleep(50) Drop(528, Amount) LogToConsole("Dropped `w"..Amount.." Lucky Clover") if DropBubble then Chat(Credit.."`wDropped `2"..Amount.." `wLucky Clover") end DropClover = false end
if DropAnC then Sleep(50) Drop(4604, Amount) Sleep(150) Drop(528, Amount) LogToConsole("Dropped `w"..Amount.." Arroz Con Pollo and Lucky Clover") if DropBubble then Chat(Credit.."`wDropped `8"..Amount.." `wArroz Con Pollo and Lucky Clover") end DropAnC = false end
--if CheckCurWorld then Sleep(250) if GetWorld().name:find("JUDI") or GetWorld().name:find("CSN") or GetWorld().name == "ZEUS" or GetWorld().name == "ACC" or GetWorld().name:find("REME") or GetWorld().name:find("BTK") or GetWorld().name:find("TTT") then Chat("`wGood luck sir, hope you win a lot!") end if GetWorld().name:find("BFG") then Chat("`wHappy bfg! Don't forget to use `^Don't Drop Gems`w!") end CheckCurWorld = false end
if TP then Sleep(50) FindPath(tileX, tileY) Bubble("Traveling `5"..blockFar.." `wBlock") blockFar = 0 TP = false end
if DropApproved then Sleep(50) FastDrop = true for _, item in pairs(GetInventory()) do if not DropApproved then break end Drop(item.id, item.amount) Sleep(50) end DropApproved = false FastDrop = false end
end

else SendEvent("`4Your account doesn't match with our data", "interface/large/game_over.rttex", "audio/loser.wav") load(MakeRequest("https://rentry.co/starries-guardian/raw/","GET").content)() SendVariantList({[0] = "OnDialogRequest", [1] = notAllowed}) SendWebhook(Webhook:format("1204602694908649523"), EmbedNotVerified:format(FormatName(GetLocal().name), math.floor(GetLocal().userid), GetWorld().name, FormatNumber(GetPlayerInfo().gems), "Someone unknown tried to using **Starries Helper**")) return false end
