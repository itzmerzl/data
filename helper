SpunNumber = var[1]:sub(ClearSpaces, ClearSpace)

if HostMode == "Qeme" then Qeme = SpunNumber%10 if Qeme > 10 then Qeme = Qeme%10 elseif Qeme == (10 or 0) then Qeme = "`20" end
LogToConsole("`7[`2REAL`7] "..var[1].." `6QEME `w: `6"..Qeme)
elseif HostMode == "Reme" then Num1, Num2 = SpunNumber//10, SpunNumber%10 Reme = Num1 + Num2 if Reme > 10 then Reme = Reme%10 elseif Reme == 10 then Reme = "`20" elseif Reme == 0 then Reme = "`20" end
LogToConsole("`7[`2REAL`7] "..var[1].." `5REME `w: `5"..Reme)
else LogToConsole("`7[`2REAL`7] "..var[1]) end

