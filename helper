-- [[ Function ]] --
function SendDropLog(itemid, amount, description) path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\Drop.txt" file = io.open(path, "a") if not file then LogToConsole("`4Oops: `4unable to create the file on the desktop.") return end file:write("Dropped "..amount.." "..GetItemInfo(itemid).name.." "..description.."\n") file:close() end
function SendCollectLog(itemid, amount, description) path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\Collect.txt" file = io.open(path, "a") if not file then LogToConsole("`4Oops: `4unable to create the file on the desktop.") return end file:write("Collected "..amount.." "..GetItemInfo(itemid).name.." "..description.."\n") file:close() end
function SendEnterLog(name, description) path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\Enter.txt" file = io.open(path, "a") if not file then LogToConsole("`4Oops: `4unable to create the file on the desktop.") return end file:write(name.." entered "..description.."\n") file:close() end
function SendExitLog(name, description) path = os.getenv("USERPROFILE") .. "\\AppData\\Local\\Growtopia\\cache\\Starries Helper\\Exit.txt" file = io.open(path, "a") if not file then LogToConsole("`4Oops: `4unable to create the file on the desktop.") return end file:write(name.." exited "..description.."\n") file:close() end

function GetRemote() for _, tile in pairs(GetTiles()) do if tile.fg == 5638 then SendPacket(2,"action|dialog_return\ndialog_name|magplant_edit\nx|"..math.floor(tile.x).."|\ny|"..math.floor(tile.y).."|\nbuttonClicked|getRemote") end end end
function ChangeBGL() for _, tile in pairs(GetTiles()) do if tile.fg == 3898 then SendPacket(2,"action|dialog_return\ndialog_name|telephone\nnum|53785|\nx|"..math.floor(tile.x).."|\ny|"..math.floor(tile.y).."|\nbuttonClicked|bglconvert") end end end
function BuyingDL() for _, tile in pairs(GetTiles()) do if tile.fg == 3898 then SendPacket(2,"action|dialog_return\ndialog_name|telephone\nnum|53785|\nx|"..math.floor(tile.x).."|\ny|"..math.floor(tile.y).."|\nbuttonClicked|dlconvert") end end end
function MergeIreng() SendPacket(2,"action|dialog_return\ndialog_name|info_box\nbuttonClicked|make_bgl") end
function ShatterIreng() SendPacket(2,"action|dialog_return\ndialog_name|info_box\nbuttonClicked|make_bluegl") end
function CollectOID(oid,x,y) SendPacketRaw(false, {type = 11, value = oid, x = x, y = y}) end 
function Collect(x,y) for _,item in pairs(GetObjectList()) do if item.pos.x // 32 == x and item.pos.y // 32 == y then CollectOID(item.oid, item.pos.x, item.pos.y) end end end 
function CollectInRange() for _, item in pairs(GetObjectList()) do CollectOID(item.oid, item.pos.x, item.pos.y) end Overlay(CollectInRangeText) end 
function CollectInCRange() if not item then Range = (Range or CustomRange) * 32 end for _, item in pairs(GetObjectList()) do CurrentPositionX, CurrentPositionY = math.abs(GetLocal().pos.x - item.pos.x), math.abs(GetLocal().pos.y - item.pos.y) if CurrentPositionX < Range and CurrentPositionY < Range then CollectOID(item.oid, item.pos.x, item.pos.y) end end end
function CountTax(bet, tax) BetPlayer, CurrentTax = tonumber(bet), tonumber(tax) CT = tonumber(CurrentTax / 100 * BetPlayer) return math.floor(BetPlayer - CT) end 
function Config(num) tonumber(num) local AmountWL, AmountDL, AmountBGL, AmountIRENG, Data = num % 100, math.floor(num / 100) % 100, math.floor(num / 10000) % 100, math.floor(num / 1000000) % 100, "`w" if AmountIRENG == 0 and AmountBGL == 0 and AmountDL == 0 and AmountWL == 0 then return "-" end if AmountIRENG ~= 0 then if AmountBGL ~= 0 or AmountDL ~= 0 or AmountWL ~= 0 then Data = Data ..AmountIRENG.. " `bBlack Gem Lock `w" else Data = Data ..AmountIRENG.. " `bBlack Gem Lock" end end if AmountBGL ~= 0 then if AmountDL ~= 0 or AmountWL ~= 0 then Data = Data ..AmountBGL.. " `eBlue Gem Lock `w" else Data = Data ..AmountBGL.. " `eBlue Gem Lock" end end if AmountDL ~= 0 then if AmountWL ~= 0 then Data = Data ..AmountDL.. " `1Diamond Lock `w" else Data = Data ..AmountDL.. " `1Diamond Lock" end end if AmountWL ~= 0 then Data = Data ..AmountWL.. " `9World Lock" end return Data end 
--function GetTiles() local r = {} for y = 0, 199, 1 do for x = 0, 199, 1 do table.insert(r, GetTile(x, y)) end end return r end

function Cdrop() tonumber(Amount) if Amount == nil then LogToConsole(AmountNilText) return true elseif Amount == 0 then LogToConsole(Amount0Text) return true end
Data = {1000000, 10000, 100, 1} IRENG, BGL, DL, WL = 0, 0, 0, 0 Res = Config(Amount)
for D = 1, 4 do while Amount >= Data[D] do if Data[D] == Data[1] then IRENG = IRENG + 1 Amount = Amount - 1000000 elseif Data[D] == Data[2] then BGL = BGL + 1 Amount = Amount - 10000 elseif Data[D] == Data[3] then DL = DL + 1 Amount = Amount - 100 elseif Data[D] == Data[4] then WL = WL + 1 Amount = Amount - 1 end end end
ItemID = {11550, 7188, 1796, 242} ItemCount = {IRENG, BGL, DL, WL}

-- [[ Get Data ]] -- 
-- LogToConsole(IRENG.." - "..BGL.." - "..DL.." - "..WL)

if GetAmount(1796) < DL and GetAmount(7188) < BGL then Bubble("`wTrying to drop "..DL.." `1Diamond Lock`w, but you don't have that many!") DontShowCdrop = true elseif GetAmount(242) < WL and GetAmount(1796) < DL then Bubble("`wTrying to drop "..WL.." `9World Lock`w, but you don't have that many!") DontShowCdrop = true end

-- [[ Shatter ]] --
if GetAmount(11550) < IRENG then MergeIreng() Sleep(150) end if GetAmount(7188) < BGL then ShatterIreng() Sleep(150) ChangeBGL() Sleep(150) end if GetAmount(1796) < DL then Shatter(7188) Sleep(150) Shatter(242) Sleep(150) end if GetAmount(242) < WL then Shatter(1796) Sleep(150) end

-- [[ Dropping ]] --
if Language == "EN" then
CdropIRENGText = "Dropped "..IRENG.." `bBlack Gem Lock"
CdropBGLText = "Dropped `w" ..BGL.. " `eBlue Gem Lock"
CdropDLText = "Dropped `w" ..DL.. " `1Diamond Lock"
CdropWLText = "Dropped `w" ..WL.. " `9World Lock"
elseif Language == "ID" then
CdropIRENGText = "Menjatuhkan "..IRENG.." `bBlack Gem Lock"
CdropBGLText = "Menjatuhkan `w" ..BGL.. " `eBlue Gem Lock"
CdropDLText = "Menjatuhkan `w" ..DL.. " `1Diamond Lock"
CdropWLText = "Menjatuhkan `w" ..WL.. " `9World Lock"
end

if IRENG > 0 then 
Drop(ItemID[1], ItemCount[1]) LogToConsole(CdropIRENGText) SendDropLog(11550, IRENG, "at "..GetWorld().name.." "..os.date("%H:%M on %d/%m"))
table.insert(ActivityLog, {drop = "\nadd_label_with_icon|small|"..GetLocal().name.." `odropped `w"..IRENG.." `bBlack Gem Lock `oin `5"..GetWorld().name.." `oat "..os.date("%H:%M on %d/%m").."|left|1400|\n", netid = GetLocal().netid, drops = "Dropped `w" ..IRENG.. " `bBlack Gem Lock"})
Sleep(50) end

if BGL > 0 then Drop(ItemID[2], ItemCount[2]) LogToConsole(CdropBGLText) SendDropLog(7188, BGL, "at "..GetWorld().name.." "..os.date("%H:%M on %d/%m"))
table.insert(ActivityLog, {drop = "\nadd_label_with_icon|small|"..GetLocal().name.." `odropped `w"..BGL.." `eBlue Gem Lock `oin `5"..GetWorld().name.." `oat "..os.date("%H:%M on %d/%m").."|left|1400|\n", netid = GetLocal().netid, drops = "Dropped `w" ..BGL.. " `eBlue Gem Lock"})
Sleep(50) end

if DL > 0 then 
Drop(ItemID[3], ItemCount[3]) LogToConsole(CdropDLText) SendDropLog(1796, DL, "at "..GetWorld().name.." "..os.date("%H:%M on %d/%m"))
table.insert(ActivityLog, {drop = "\nadd_label_with_icon|small|"..GetLocal().name.." `odropped `w"..DL.." `1Diamond Lock `oin `5"..GetWorld().name.." `oat "..os.date("%H:%M on %d/%m").."|left|1400|\n", netid = GetLocal().netid, drops = "Dropped `w" ..DL.. " `1Diamond Lock"})
Sleep(50) end

if WL > 0 then 
Drop(ItemID[4], ItemCount[4]) LogToConsole(CdropWLText) SendDropLog(242, WL, "at "..GetWorld().name.." "..os.date("%H:%M on %d/%m")) 
table.insert(ActivityLog, {drop = "\nadd_label_with_icon|small|"..GetLocal().name.." `odropped `w"..WL.." `9World Lock `oin `5"..GetWorld().name.." `oat "..os.date("%H:%M on %d/%m").."|left|1400|\n", netid = GetLocal().netid, drops = "Dropped `w" ..WL.. " `9World Lock"})
Sleep(50) end 

-- [[ Showing Dropped ]] --

if not DontShowCdrop then if Language == "EN" then if DropBubble then Chat(Credit.."`wDropped "..Res) end elseif Language == "ID" then if DropBubble then Chat(Credit.."`wMenjatuhkan "..Res) end end else DontShowCdrop = false return true end

end

function TimeSplit(t) local h = math.floor((t % 86400) / 3600) local m = math.floor((t % 3600) / 60) local s = math.floor((t % 60)) return h.." %s "..m.." %s "..s.." %s" end

function Broadcasting()
	if Language == "EN" then
	Broadcast1 = "Broadcast will be sent in countdown" Broadcast2 = "Broadcasting completed! total broadcast" Broadcast3 = "Thanks"
	Broadcast4 = "Broadcasting" Broadcast5 = "out of" Broadcast6 = "total broadcast" Broadcast7 = "remaining" Broadcast8 = "to" Broadcast9 = "broadcast predictions" Broadcast10 = "Remaining time" Broadcast11 = "Time spent"
	BroadcastH, BroadcastM, BroadcastS = "hours", "minutes", "seconds"
	elseif Language == "ID" then
	Broadcast1 = "Siaran akan dikirim dalam hitungan mundur" Broadcast2 = "Penyiaran selesai! jumlah siaran" Broadcast3 = "Terimakasih"
	Broadcast4 = "Penyiaran" Broadcast5 = "dari" Broadcast6 = "jumlah siaran" Broadcast7 = "tersisa" Broadcast8 = "ke" Broadcast9 = "prediksi siaran" Broadcast10 = "Waktu tersisa" Broadcast11 = "Waktu dihabiskan"
	BroadcastH, BroadcastM, BroadcastS = "jam", "menit", "detik"
	end
	if BroadcastMode == "Counter" then CheckCounter = true
		if not AlreadyBroadcast then
			if CounterMode == "Up" then
				local Message = Credit.."`w(megaphone) "..Broadcast1.." : `%s" Chat(Message:format(53)) Sleep(1000) Chat(Message:format(52)) Sleep(1000) Chat(Message:format(51)) Sleep(1500)
				Chat("/sb "..BroadcastText.."") Sleep(1000) CurrentBroadcast = CurrentBroadcast + 1 
				if CheckCounter and (CounterBroadcast-CurrentBroadcast == 0 ) then CheckCounter = false Chat(Credit.."`w(nuke) "..Broadcast2.." : `6"..CounterBroadcast.."`w, "..Broadcast3.."! (halo)") Sleep(1000) if MentionUser or MentionMode == "End" and MentionMode == "All" then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, TimeSplit(0):format("hours,", "minutes,", "seconds."), "Super Broadcasting finished, enjoy your break!", BroadcastText)) BroadcastGemsUsed, CurrentBroadcast = 0, 0 if BackWorld then Chat(Credit.."(fireworks) `wBack to "..BroadcastWorld) Sleep(1000) RequestJoinWorld(BroadcastWorld) end StartingSuperBroadcast = false AlreadyBroadcast = false return true else Chat(Credit.."`w(eyes) "..Broadcast4.." `2"..CurrentBroadcast.." `w"..Broadcast5.." `6"..CounterBroadcast.." `w"..Broadcast6..", `e"..CounterBroadcast-CurrentBroadcast.." `w"..Broadcast7..".") Sleep(1000) if MentionUser and MentionMode == "All" then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, TimeSplit(0):format("hours,", "minutes,", "seconds."),"Super Broadcasting! **"..CounterBroadcast-CurrentBroadcast.."** broadcast left until it stopped", BroadcastText)) AlreadyBroadcast = true end
				Sleep(TimerQueue)
			end

			if CounterMode == "Down" then
				local Message = Credit.."`w(megaphone) "..Broadcast1.." : `%s" Chat(Message:format(53)) Sleep(1000) Chat(Message:format(52)) Sleep(1000) Chat(Message:format(51)) Sleep(1500)
				Chat("/sb "..BroadcastText.."") Sleep(1000) CounterBroadcast = CounterBroadcast - 1 
				if CheckCounter and (CurrentBroadcast-CounterBroadcast == 0 ) then CheckCounter = false Chat(Credit.."`w(nuke) "..Broadcast2.." : `6"..CurrentBroadcast.."`w, "..Broadcast3.."! (halo)") Sleep(1000) if MentionUser or MentionMode == "End" and MentionMode == "All" then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, TimeSplit(0):format("hours,", "minutes,", "seconds."), "Super Broadcasting finished, enjoy your break!", BroadcastText)) BroadcastGemsUsed, CurrentBroadcast = 0, 0 if BackWorld then Chat(Credit.."(fireworks) `wBack to "..BroadcastWorld) Sleep(1000) RequestJoinWorld(BroadcastWorld) end StartingSuperBroadcast = false AlreadyBroadcast = false return true else Chat(Credit.."`w(eyes) "..Broadcast4.." `2"..CounterBroadcast.." `w"..Broadcast8.." `6"..CurrentBroadcast.." `w"..Broadcast6..", `e"..CurrentBroadcast-CurrentBroadcast.." `w"..Broadcast7..".") Sleep(1000) if MentionUser and MentionMode == "All" then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, TimeSplit(0):format("hours,", "minutes,", "seconds."),"Super Broadcasting! **"..CurrentBroadcast-CounterBroadcast.."** broadcast left until it stopped", BroadcastText)) AlreadyBroadcast = true end
				Sleep(TimerQueue)
			end
		end
	end
	if BroadcastMode == "Timer" then CheckTime = true
		CurrentTime = os.time() GetTime = os.difftime(CurrentTime, TimeStart) Time = (BroadcastTime * 60) - GetTime
		if not AlreadyBroadcast then
			local Message = Credit.."`w(megaphone) "..Broadcast1.." : `%s" Chat(Message:format(53)) Sleep(1000) Chat(Message:format(52)) Sleep(1000) Chat(Message:format(51)) Sleep(1000)
			Chat("/sb "..BroadcastText.."") Sleep(1000) CurrentBroadcast = CurrentBroadcast + 1 
			Chat(Credit.."`w(eyes) "..Broadcast4.." `2"..CurrentBroadcast.." `w"..Broadcast5.." `6"..math.floor(BroadcastTime/1.5).." `w"..Broadcast9) Sleep(1000) Chat(Credit.."`w(sleep) "..Broadcast10.." : `b"..TimeSplit(Time):format("`w"..BroadcastH..",`b", "`w"..BroadcastM..",`b", "`w"..BroadcastS..".")) Sleep(1000) if MentionUser and MentionMode == "All" then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, TimeSplit(Time):format("hours,", "minutes,", "seconds."),"Super Broadcasting! **"..(math.floor(BroadcastTime/1.5) - CurrentBroadcast).."** broadcast left until it stopped", BroadcastText)) AlreadyBroadcast = true return true
		end
		if CheckTime and Time <= 0 then CheckTime = false Chat(Credit.."`w(nuke) "..Broadcast2.." : `6"..CurrentBroadcast.."`w, "..Broadcast3.."! (halo)") Sleep(1000) if MentionUser and MentionMode == "End" or MentionMode == "All" then Mentioned = "<@"..BroadcastDiscordID..">" else Mentioned = "" end SendWebhook(BroadcastWebhookLink, EmbedBroadcast:format(Mentioned, FormatName(GetLocal().name), GetWorld().name, FormatNumber(GetPlayerInfo().gems), FormatNumber(BroadcastGemsUsed), CurrentBroadcast, TimeSplit(0):format("hours,", "minutes,", "seconds."), "Super Broadcasting finished, enjoy your break!", BroadcastText)) BroadcastGemsUsed, CurrentBroadcast = 0, 0 StartingSuperBroadcast = false Chat(Credit.."`w(sleep) "..Broadcast11.." : `b"..TimeSplit(BroadcastTime*60):format("`w"..BroadcastH..",`b", "`w"..BroadcastM..",`b", "`w"..BroadcastS..".")) Sleep(1000) if BackWorld then Chat(Credit.."(fireworks) `wBack to "..BroadcastWorld) Sleep(1000) RequestJoinWorld(BroadcastWorld) end return true end
	end
end

function State(x, y, z) local facing = -1 if z == "left" then facing = 48 elseif z == "right" then facing = 32 end SendPacketRaw(false, {type = 0, x = (x) * 32, y = (y) * 32, state = facing}) Sleep(50) end
function GetName(id) for _, player in pairs(GetPlayerList()) do if player.netid == id then return player.name end end end 
function GetNetid(name) for _, player in pairs(GetPlayerList()) do if player.name == name then return player.netid end end end 
function Divider(str, sub) local T = {} local A = "(.-)" .. sub local L = 1 local B, C, D = str:find(A, 1) while B do if B ~= 1 or D ~= "" then table.insert(T,D) end L = C+1 B, C, D = str:find(A, L) end if L then D = str:sub(L) table.insert(T, D) end return T end 
function Place(id, x, y) SendPacketRaw(false,{type = 3, value = id, x = GetLocal().pos.x, y = GetLocal().pos.y, px = x, py = y}) end 
function Shatter(id) SendPacketRaw(false, {type = 10, value = id}) end 
function Drop(itemID, count) SendPacket(2,"action|dialog_return\ndialog_name|drop\nitem_drop|".. itemID .. "|\nitem_count|".. count) Sleep(150) end 
function Trash(itemID, count) SendPacket(2,"action|dialog_return\ndialog_name|trash\nitem_trash|".. itemID .. "|\nitem_count|".. count) Sleep(150) end 
function UseTools(commandid) SendPacket(2,"action|dialog_return\ndialog_name|surgery\nbuttonClicked|command_"..commandid) end
function CheckWear(id) for _, item in pairs(GetInventory()) do if item.id == id then return math.floor(item.flags) end end return 0 end
function SetNickname(str, netid) SendVariantList({[0] = "OnNameChanged", [1] = str, netid = netid}) end
function ToggleCheckbox(boolean) local result = "" if boolean then result =  "1" else result = "0" end return result end
function SetFrame(boolean) local result = "" if boolean == true then result =  "staticPurpleFrame" else result = "staticGreyFrame" end return result end 
function SetFrameMode(var, str) local result = "" if var == str then result = "staticPurpleFrame" else result = "staticGreyFrame" end return result end 

function TakeBet() if Posx1 == 0 and Posx2 == 0 then LogToConsole(Oops.."Set Position 1 & Position 2 first with command `5/pos1-2") return true end DroptoPos1, DroptoPos2 = 0, 0 for _, objects in pairs(GetObjectList()) do if objects.pos.x//32 == Posx1 and objects.pos.y//32 == Posy1 then if objects.id == 242.0 then DroptoPos1 = DroptoPos1 + objects.amount end if objects.id == 1796.0 then DroptoPos1 = DroptoPos1 + objects.amount * 100 end if objects.id == 7188.0 then DroptoPos1 = DroptoPos1 + objects.amount * 10000 end if objects.id == 11550.0 then DroptoPos1 = DroptoPos1 + objects.amount * 1000000 end end if objects.pos.x//32 == Posx2 and objects.pos.y//32 == Posy2 then if objects.id == 242.0 then DroptoPos2 = DroptoPos2 + objects.amount end if objects.id == 1796.0 then DroptoPos2 = DroptoPos2 + objects.amount * 100 end if objects.id == 7188.0 then DroptoPos2 = DroptoPos2 + objects.amount * 10000 end if objects.id == 11550.0 then DroptoPos2 = DroptoPos2 + objects.amount * 1000000 end end end DroptoPos1, DroptoPos2 = math.floor(DroptoPos1), math.floor(DroptoPos2) if DroptoPos1 ~= DroptoPos2 then LogToConsole(Oops.."The amount of bet are'nt equal, make sure to drop same bet!") Chat("The amount of bet are'nt equal, make sure to drop same bet!") return true end LogToConsole("Bet : `2"..Config(DroptoPos1).. " `oI Tax : `2"..Tax1.."`o% I Drop : `2"..Config(CountTax(DroptoPos2 * 2, Tax1))) Overlay("Both bets are equal\n"..Tax1.."% Tax\nPlayer 1 : "..Config(DroptoPos1) .. "\n`wPlayer 2 : " .. Config(DroptoPos2) .. " \n`wAmount to drop : " .. Config(CountTax(DroptoPos2 * 2, Tax1))) Bet = DroptoPos1 Collect(Posx1-1, Posy1) Collect(Posx1, Posy1) Collect(Posx1+1, Posy1) Collect(Posx2-1, Posy2) Collect(Posx2, Posy2) Collect(Posx2+1, Posy2) Chat(AfterCollectBetBubbleText) return true end
function SetWinner(Choice) 
if Posx1 == 0 and Posx2 == 0 then LogToConsole(Oops.."Set Position 1 & Position 2 first with command `5/pos1-2") return true end 
if Hostx == 0 and Hosty == 0 then LogToConsole(Oops.."Set position hoster first with command `5/posh") return true end 
if Bet == 0 then LogToConsole(Oops.."Set amount bet with command `5/bet <amount>") return true end 
if Choice == 1 then State(Posx1, Posy1, "left") Amount = CountTax(Bet * 2, Tax1) AutoTax = true Bet = 0 elseif Choice == 2 then State(Posx2, Posy2, "right") Amount = CountTax(Bet * 2, Tax1) AutoTax = true Bet = 0 end
if not Horizontal and not Vertical then LogToConsole(Oops.."To automatically put chand, set position chand first with command `5/btkmenu") return true end
if Horizontal then ResetChand1 = true elseif Vertical then ResetChand2 = true end
end 

function BackBet() if Posx1 == 0 and Posx2 == 0 then LogToConsole(Oops.."Set Position 1 & Position 2 first with command `5/pos1-2") return true end if Bet == 0 then LogToConsole(Oops.."Set amount bet with command `5/bet <amount>") return true end DontShowCdrop = true State(Posx1, Posy1, "left") Amount = Bet Cdrop() DontShowCdrop = true Sleep(100) State(Posx2, Posy2, "right") Amount = Bet Chat(Credit.."`wGiving back the player bet : "..Config(Amount)) Cdrop() end 

function ScanGems() if Posxgem1 == 0 and Posxgem2 == 0 then LogToConsole(Oops.."Setting position chand or take gems first!") return true end Count = 0 DataGems = {} for _, obj in pairs(GetObjectList()) do for _, tiles in pairs(Tiles.Pos1) do if (obj.id == 112) and (obj.pos.x//32 == tiles.x) and (obj.pos.y//32 == tiles.y) then Count = math.floor(Count + obj.amount) break end end end table.insert(DataGems, Count) Count = 0 for _, obj in pairs(GetObjectList()) do for _, tiles in pairs(Tiles.Pos2) do if (obj.id == 112) and ((obj.pos.x//32) == tiles.x) and ((obj.pos.y//32) == tiles.y) then Count = math.floor(Count + obj.amount) break end end end table.insert(DataGems, Count) Count = 0 if DataGems[1] > DataGems[2] then Chat("`7[`2Win`7] `wLeft : `2"..DataGems[1].." (gems) `7Vs `wRight : `4"..DataGems[2].." (gems) `7[`4Lose`7]") elseif DataGems[1] == DataGems[2] then Chat("`7[`1Draw`7] `wLeft : `1"..DataGems[1].." (gems) `7Vs `wRight : `1"..DataGems[2].." (gems) `7[`1Draw`7]") elseif DataGems[1] < DataGems[2] then Chat("`7[`4Lose`7] `wLeft : `4"..DataGems[1].." (gems) `7Vs `wRight : `2"..DataGems[2].." (gems) `7[`2Win`7]") end DataGems = {} end
function CollectGems() if Posxgem1 == 0 and Posxgem2 == 0 then LogToConsole(Oops.."Setting position chand or take gems first!") return true end Count = 0 DataGems = {} for _, obj in pairs(GetObjectList()) do for _, tiles in pairs(Tiles.Pos1) do if (obj.id == 112) and (obj.pos.x//32 == tiles.x) and (obj.pos.y//32 == tiles.y) then Count = math.floor(Count + obj.amount) CollectOID(obj.oid, obj.pos.x, obj.pos.y) end end end table.insert(DataGems, Count) Count = 0 for _, obj in pairs(GetObjectList()) do for _, tiles in pairs(Tiles.Pos2) do if (obj.id == 112) and ((obj.pos.x//32) == tiles.x) and ((obj.pos.y//32) == tiles.y) then CollectOID(obj.oid, obj.pos.x, obj.pos.y) Count = math.floor(Count + obj.amount) end end end table.insert(DataGems, Count) Count = 0 if DataGems[1] > DataGems[2] then Chat("`7[`2Win`7] `wLeft : `2"..DataGems[1].." (gems) `7Vs `wRight : `4"..DataGems[2].." (gems) `7[`4Lose`7]") Winner = 1 elseif DataGems[1] == DataGems[2] then Chat("`7[`1Draw`7] `wLeft : `1"..DataGems[1].." (gems) `7Vs `wRight : `1"..DataGems[2].." (gems) `7[`1Draw`7]") Winner = 0 elseif DataGems[1] < DataGems[2] then Chat("`7[`4Lose`7] `wLeft : `4"..DataGems[1].." (gems) `7Vs `wRight : `2"..DataGems[2].." (gems) `7[`2Win`7]") Winner = 2 end DataGems = {} end
function CollectChandPos1() for _, obj in pairs(GetObjectList()) do for _, tiles in pairs(Tiles.Pos1) do if (obj.pos.x//32 == tiles.x) and (obj.pos.y//32 == tiles.y) then CollectOID(obj.oid, obj.pos.x, obj.pos.y) end end end end
function CollectChandPos2() for _, obj in pairs(GetObjectList()) do for _, tiles in pairs(Tiles.Pos2) do if (obj.pos.x//32 == tiles.x) and (obj.pos.y//32 == tiles.y) then CollectOID(obj.oid, obj.pos.x, obj.pos.y) end end end end
function StartSpam() if ColorfulSpam then Chat("`"..Randomize[math.random(1, #Randomize)]..""..SpamText) Sleep(SpamDelay) else Chat(SpamText) Sleep(SpamDelay) end end 
function SkinnyBlink() local RandomSkin = {1685231359, 2022356223, 2190853119, 2527912447, 2864971775, 3033464831, 3370516479, 1348237567, 2749215231, 3317842431, 726390783, 713703935, 3578898943, 4042322175, 3531226367, 4023103999, 194314239, 1345519520} SendPacket(2,"action|setSkin\ncolor|".. RandomSkin[math.random(#RandomSkin)]); Sleep(150); end
function SendSpin() if FakeSpin then FakeSpinNum = {"`b18", "`418", "`b19", "`419", "`b27", "`427", "`b28", "`428", "`b36", "`436", "`20"} Chat("`w["..GetLocal().name.." `wspun the wheel and got "..FakeSpinNum[math.random(1, #FakeSpinNum)].."`w!]") else LogToConsole("`4Oops: `olooks like auto fake spin is currently `4disabled`o, to activate it run command `5/fs") end end 
function FilterSpin(id) FilterLog = {} for _, log in pairs(LogSpin) do if log.netid == id then table.insert(FilterLog,"\nadd_label_with_icon|small|"..log.spin.."|left|758|\n") end end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|"..GetName(id).." Spin|left|1400|\nadd_spacer|small|\n"..table.concat(FilterLog).."\nadd_spacer|small|\nadd_button|/cctv|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|FilterSpin|||"}) end
function SpinLog() SpunLog = {} for _, spin in pairs(LogSpin) do table.insert(SpunLog, spin.spin) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Spin Log|left|10258|\nadd_spacer|small|\nadd_smalltext|Click the wheel button to filter player spun|\n"..table.concat(SpunLog).."\nadd_spacer|small|\nadd_quick_exit|||\nend_dialog|WorldSpin|Close||"}) end
function ArrowScan(id, status) pkt = { type = 37, netid = id } if status then pkt.dropped = 1 end SendPacketRaw(true, pkt) end

function FilterWrench(WrenchType) FilterLog = {} for _, log in pairs(ActivityLog) do if log.wrenchtype == WrenchType then table.insert(FilterLog,"\nadd_label_with_icon|small|"..log.wrench.."|left|758|\n") end end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|"..WrenchType.." Log|left|1400|\nadd_spacer|small|\n"..table.concat(FilterLog).."\nadd_spacer|small|\nadd_button|/actwrench|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|FilterWrench|||"}) end
function WrenchLog() WrenchAction = {} for _, act in pairs(ActivityLog) do table.insert(WrenchAction, act.wrench) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Wrench Log|left|32|\nadd_spacer|small|\n"..table.concat(WrenchAction).."\nadd_spacer|small|\nadd_button|/log|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|ActivityWrenchLog|||"}) end
function DropLog() DropAction = {} for _, act in pairs(ActivityLog) do table.insert(DropAction, act.drop) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Dropped Log|left|13810|\nadd_spacer|small|\n"..table.concat(DropAction).."\nadd_spacer|small|\nadd_button|/log|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|ActivityDropLog|||"}) end
function TrashLog() TrashAction = {} for _, act in pairs(ActivityLog) do table.insert(TrashAction, act.trash) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Trashed Log|left|13810|\nadd_spacer|small|\n"..table.concat(TrashAction).."\nadd_spacer|small|\nadd_button|/log|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|ActivityTrashLog|||"}) end

function CollectLog() CollectAction = {} for _, act in pairs(ActivityLog) do table.insert(CollectAction, act.collect) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Collected Log|left|13808|\nadd_spacer|small|\n"..table.concat(CollectAction).."\nadd_spacer|small|\nadd_button|/log|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|ActivityCollectLog|||"}) end
function EnterLog() EnterAction = {} for _, act in pairs(ActivityLog) do table.insert(EnterAction, act.enter) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Entered Log|left|13804|\nadd_spacer|small|\n"..table.concat(EnterAction).."\nadd_spacer|small|\nadd_button|/log|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|ActivityEnterLog|||"}) end
function ExitLog() ExitAction = {} for _, act in pairs(ActivityLog) do table.insert(ExitAction, act.exit) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Exited Log|left|13806|\nadd_spacer|small|\n"..table.concat(ExitAction).."\nadd_spacer|small|\nadd_button|/log|Back|noflags|0|0|\nadd_quick_exit|||\nend_dialog|ActivityExitLog|||"}) end

WorldTYPE = type(GetTile(199, 199)) == "table"
PosX = WorldTYPE and 199 or 99
PosY = WorldTYPE and 194 or 58

function GetTiles() local r = {} for y = 0, PosY, 1 do for x = 0, PosX, 1 do table.insert(r, GetTile(x, y)) end end return r end

function GetObjects() local old_obj = GetObjectList() local ret_obj = {} for _, v in pairs(old_obj) do table.insert(ret_obj, v) end table.sort(ret_obj, function(left, right) return (left.pos.y < right.pos.y) or (left.pos.y == right.pos.y and left.pos.x < right.pos.x) end) return ret_obj end

function GetFloating() for _, obj in pairs(GetObjectList()) do if not GettingFloat then break return end FindPath(math.floor(obj.pos.x/32),math.floor(obj.pos.y/32), 100) end LogToConsole("There's no floating item in this world") GettingFloat = false LogToConsole("Getting floating `4Disabled") ChangeValue("[C] Modfly", false) end
function ClearWater() if GetAmount(822) < 1 then LogToConsole("You don't have any water bucket!") return true end for x = 0, PosX do for y = 0, PosY do if not ClearingWater then break return end if GetTile(x,y).flags.water then FindPath(x,y,100) Sleep(150) Place(822, math.floor(GetLocal().pos.x/32), math.floor(GetLocal().pos.y/32)) Sleep(150) end end end LogToConsole("I didn't found water in this world") ClearingWater = false ChangeValue("[C] Modfly", false) end
function BreakingPlatform() for y = 0, PosY do for x = 0, PosX do if not BreakPlatform then break return end if (GetTile(x, y).fg == PlatformID) then FindPath(x, y, 150) Place(18, x, y) Sleep(150) end end end LogToConsole("There are no longer any platforms or blocks") BreakPlatform = false LogToConsole("Breaking platform `4Disabled") end
function BuildingPlatform() for y = 1, PosY, 2 do for x = 0, PosX do if not BuildPlatform then break return end if GetTile(x,y).fg == 0 then FindPath(x-1, y, 150) Place(PlatformID, x, y) Sleep(150) if GetAmount(PlatformID) == 0 then SendPacket(2,"action|dialog_return\ndialog_name|item_search\n"..PlatformID.."|1") Sleep(500) end end LogToConsole("There are no longer room for platform or block") BuildPlatform = false LogToConsole("Building platform `4Disabled") end end end

