-- [[ Function ]] --
function CheckUid(Userid, Local) for _,value in pairs(Userid) do if value == Local then return true end end return false end
function SendWebhook(url, data) MakeRequest(url,"POST",{["Content-Type"] = "application/json"},data) end
function Warn(str) SendVariantList({[0] = "OnAddNotification", [1] = "interface/atomic_button.rttex", [2] = str, [3] = "audio/hub_open.wav"}) end
function SendEvent(str, effect, audio) SendVariantList({[0] = "OnAddNotification", [1] = effect, [2] = str, [3] = audio}) end
function LogToConsole(str) SendVariantList({[0] = "OnConsoleMessage", [1] = Credit..""..str }) end 
function Chat(str) SendPacket(2, "action|input\n|text|" .. str) end 
function Bubble(str) SendVariantList({[0] = "OnTalkBubble", [1] = GetLocal().netid, [2] = str}) return true end
function Overlay(str) SendVariantList({[0] = "OnTextOverlay", [1] = str}) end 
function FormatNumber(num) num = math.floor(num + 0.5) local formatted = tostring(num) local n = 3 while n < #formatted do formatted = formatted:sub(1, #formatted - n) .. "." .. formatted:sub(#formatted - n + 1) n = n + 4 end return formatted end
function GetAmount(id) for _, item in pairs(GetInventory()) do if item.id == id then return math.floor(item.amount) end end return 0 end
function FloatingAmount(id) Count = 0 for _, object in pairs(GetObjectList()) do if object.id == id then Count = Count + object.amount end end return Count end
function ShowDialog(dialog, delay) SendVariantList({[0] = "OnDialogRequest", [1] = dialog}, -1, delay) end
function GetPlayerCount() Count = 0 for _, player in pairs(GetPlayerList()) do Count = Count + 1 end return Count end
function FormatName(str) str = str:gsub("``","") str = str:gsub("`.","") str = str:gsub("Dr.","") str = str:gsub("@",""):gsub(" of Legend",""):gsub("%[BOOST%]","") str = str:gsub("%[ELITE%]",""):gsub(" ","") return str end

