CollectTable = {} function ShowColek() Collecting = {} CollectWL, CollectDL, CollectBGL, CollectIRENG = 0, 0, 0, 0 for _, act in pairs(CollectTable) do table.insert(Collecting, act.num) break return end Sleep(150) if table.concat(Collecting) ~= nil and table.concat(Collecting) ~= "" then Chat(Credit.."`wCollected "..Config(table.concat(Collecting))) end CollectTable = {} Collecting = {} ShowColekted = false end 

