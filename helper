function ScanBlocks()
DataScan = {}
for _, tile in pairs(GetTiles()) do value = tile.fg
if DataScan[tile.fg] == nil then DataScan[tile.fg] = {value = tile.fg, amount = 1} else DataScan[tile.fg].amount = DataScan[tile.fg].amount + 1 end
if DataScan[tile.bg] == nil then DataScan[tile.bg] = {value = tile.bg, amount = 1} else DataScan[tile.bg].amount = DataScan[tile.bg].amount + 1 end
end
GimmeSpace = "add_spacer|small|"
for _, data in pairs(DataScan) do
ItemCount = math.floor(data.amount)
ItemName = GetItemInfo(math.floor(data.value)).name
if math.floor(data.value) ~= 0 then
GimmeSpace = GimmeSpace.."\nadd_label_with_icon_button_list|small|"..ItemName.." : "..ItemCount.."|left|ScanThisBlock_|itemID_itemAmount|"..math.floor(data.value)..","..ItemCount
end end
Sleep(500)
SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|World Blocks|left|6016\n"..GimmeSpace.."\nadd_quick_exit\nadd_spacer|small\nadd_button|/gscan|Back|noflags|0|0|\nend_dialog|StarriesHelper|||"})
end

function ScanFloats()
DataScan = {}
for _, object in pairs(GetObjectList()) do value = object.id
if DataScan[object.id] == nil then DataScan[object.id] = {value = object.id, amount = object.amount} else DataScan[object.id].amount = DataScan[object.id].amount + object.amount end
end
GimmeSpace = "add_spacer|small|"
for _, data in pairs(DataScan) do
ItemCount = math.floor(data.amount)
ItemName = GetItemInfo(math.floor(data.value)).name
GimmeSpace = GimmeSpace.."\nadd_label_with_icon_button_list|small|"..ItemName.." : "..ItemCount.."|left|ScanThisFloat_|itemID_itemAmount|"..math.floor(data.value)..","..ItemCount
end
Sleep(500)
SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Floating Items|left|6016\n"..GimmeSpace.."\nadd_quick_exit\nadd_spacer|small|\nadd_button|/gscan|Back|noflags|0|0|\nend_dialog|StarriesHelper|||"})
end

function ScanInventory()
DataScan = {}
for _, inv in pairs(GetInventory()) do value = inv.id
if DataScan[inv.id] == nil then DataScan[inv.id] = {value = inv.id, amount = inv.amount} else DataScan[inv.id].amount = DataScan[inv.id].amount + inv.amount end
end
GimmeSpace = "add_spacer|small|"
for _, data in pairs(DataScan) do
ItemCount = math.floor(data.amount)
ItemName = GetItemInfo(math.floor(data.value)).name
GimmeSpace = GimmeSpace.."\nadd_label_with_icon_button_list|small|"..ItemName.." : "..ItemCount.."|left|DropThisItem_|itemID_itemAmount|"..math.floor(data.value)..","..ItemCount
end
Sleep(500)
SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`"..PrimaryColor.."|\nadd_label_with_icon|big|Inventory|left|448\n"..GimmeSpace.."\nadd_quick_exit\nadd_spacer|small|\nend_dialog|StarriesHelper|Close||"})
end

