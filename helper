 end 
  
 function user_deinvis() 
   SendVariant({ 
     [0] = "OnSetClothing", 
     [1] = {0,0,0}, 
     [2] = {0,0,0}, 
     [3] = {0,0,0}, 
     [4] = -1, 
     [5] = {0, 0, 0}, 
   },GetLocal().netid,100) 
 end 

if Command:find("/invis") then SendVariantList({[0] = "OnSetClothing", [1] = {0,0,0}, [2] = {0,0,0}, [3] = {0,0,0}, [4] = 2, [5] = {0, 0, 0}}, GetLocal().netid,100)  return true end
