if var[0] == "OnConsoleMessage" and (var[1]:find("Collected") and (var[1]:match("Black Gem Lock") or var[1]:match("Blue Gem Lock") or var[1]:match("Diamond Lock") or var[1]:match("World Lock"))) then
if var[1]:find("Collected  `w(%d+) Black Gem Lock") then if CollectIRENG == nil or CollectIRENG == 0 then CollectIRENG = 0 end if CollectBGL == nil or CollectBGL == 0 then CollectBGL = 0 end if CollectDL == nil or CollectDL == 0 then CollectDL = 0 end if CollectWL == nil or CollectWL == 0 then CollectWL = 0 end
CollectIRENG = tonumber(var[1]:match("Collected  `w(%d+) Black Gem Lock")) SendCollectLog(11550, CollectIRENG, "from "..GetWorld().name.." "..os.date("%H:%M on %d/%m"))
table.insert(CollectTable, {num = (CollectIRENG*1000000)+(CollectBGL*10000)+(CollectDL*100)+(CollectWL)})
table.insert(ActivityLog, {collect = "\nadd_label_with_icon|small|"..GetLocal().name.." `otook `w"..CollectIRENG.." `bBlack Gem Lock `ofrom `5"..GetWorld().name.." `oat "..os.date("%H:%M on %d/%m").."|left|18|\n", netid = GetLocal().netid})
