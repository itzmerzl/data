if WrenchMode == "Kick" and var[1]:find("add_button|kick") then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..NetID.."|\nbuttonClicked|kick") table.insert(ActivityLog, {wrench = "\nadd_label_with_icon_button|small|"..GetLocal().name.." `okicking "..Name.." `oat `5"..GetWorld().name.." `o"..os.date("%H:%M on %d/%m").."|left|732|"..WrenchMode.."|\n", netid = GetLocal().netid, wrenchtype = WrenchMode}) Overlay("Kicking "..Name) return true end
if WrenchMode == "Ban" and var[1]:find("add_button|world_ban") then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..NetID.."|\nbuttonClicked|world_ban") table.insert(ActivityLog, {wrench = "\nadd_label_with_icon_button|small|"..GetLocal().name.." `obanning "..Name.." `oat `5"..GetWorld().name.." `o"..os.date("%H:%M on %d/%m").."|left|278|"..WrenchMode.."|\n", netid = GetLocal().netid, wrenchtype = WrenchMode}) Overlay("Banning "..Name) return true end 
if WrenchMode == "Surgery" and var[1]:find("add_button|surgery") then if (GetAmount(1258) or GetAmount(1260) or GetAmount(1262) or GetAmount(1264) or GetAmount(1266) or GetAmount(1268) or GetAmount(1270)) < 20 then LogToConsole("`4BE CAREFUL! `oYour surgical tools will run out soon!") return true end SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..NetID.."|\nbuttonClicked|surgery") table.insert(ActivityLog, {wrench = "\nadd_label_with_icon_button|small|"..GetLocal().name.." `ooperating "..Name.." `oat `5"..GetWorld().name.." `o"..os.date("%H:%M on %d/%m").."|left|1296|"..WrenchMode.."|\n", netid = GetLocal().netid, wrenchtype = WrenchMode}) return true end 
if WrenchMode == "Pvp" and var[1]:find("add_button|pvp") then SendPacket(2,"action|dialog_return\ndialog_name|pvp_settings\nnetID|"..NetID.."|\nbet|50") table.insert(ActivityLog, {wrench = "\nadd_label_with_icon_button|small|"..GetLocal().name.." `ofighting "..Name.." `oat `5"..GetWorld().name.." `o"..os.date("%H:%M on %d/%m").."|left|278|"..WrenchMode.."|\n", netid = GetLocal().netid, wrenchtype = WrenchMode}) Overlay("Fighting "..Name) return true end 
if WrenchMode == "Presence" then Chat("`w(grin) Hi "..Name.." `wnice to meet you!") table.insert(ActivityLog, {wrench = "\nadd_label_with_icon_button|small|"..GetLocal().name.." `ogreeting "..Name.." `oat `5"..GetWorld().name.." `o"..os.date("%H:%M on %d/%m").."|left|278|"..WrenchMode.."|\n", netid = GetLocal().netid, wrenchtype = WrenchMode}) Overlay("Greeting "..Name) return true end 
if WrenchMode == "Freeze" and var[1]:find("add_button|freeze") then SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..NetID.."|\nbuttonClicked|freeze") table.insert(ActivityLog, {wrench = "\nadd_label_with_icon_button|small|"..GetLocal().name.." `ofreezing "..Name.." `oat `5"..GetWorld().name.." `o"..os.date("%H:%M on %d/%m").."|left|278|"..WrenchMode.."|\n", netid = GetLocal().netid, wrenchtype = WrenchMode}) Overlay("Freezing "..Name) return true end 
end
end

end

if AutoSurgery then 
if var[0] == "OnConsoleMessage" then
if var[1]:find("Malpractice") then Chat("/modage 60") return true end
if var[1]:find("Recovering fom surgery") then Chat("reco") return true end
end
if var[0] == "OnDialogRequest" then
if var[1]:find("Anatomical") then x = var[1]:match("embed_data|x|(%d+)") y = var[1]:match("embed_data|y|(%d+)") SendPacket(2,"action|dialog_return\ndialog_name|surge_edit\nx|"..x.."|\ny|"..y.."|\n") return true
elseif var[1]:find("Are you sure you want to perform surgery on this robot?") then x = var[1]:match("embed_data|x|(%d+)") y = var[1]:match("embed_data|y|(%d+)") SendPacket(2,"action|dialog_return\ndialog_name|surge_edit\nx|"..x.."|\ny|"..y.."|\n")
elseif var[1]:find("Perform Surgery") then SendPacket(2, "action|dialog_return\ndialog_name|popup\nnetID|"..TargetSurg.."|\nbuttonClicked|surgery") return true
elseif var[1]:find("Patient's fever is `3slowly rising") and var[1]:find("command_4") then UseTools(4) LogToConsole("Using Antibiotic") return true
elseif var[1]:find("Patient's fever is `6climbing") and var[1]:find("command_4") then UseTools(4) LogToConsole("Using Antibiotic") return true
elseif var[1]:find("Incisions: `60") and var[1]:find("command_7") then UseTools(7) LogToConsole("Fix It") return true
elseif var[1]:find("Incisions: `30") and var[1]:find("command_7") then UseTools(7) LogToConsole("Fix It") return true
elseif var[1]:find("command_7") then UseTools(7) LogToConsole("Fix It") return true
elseif var[1]:find("Operation site: `6Unclean") and var[1]:find("command_3") then UseTools(3) LogToConsole("Using Antiseptic") return true
elseif var[1]:find("Operation site: `4Unsanitary") and var[1]:find("command_3") then UseTools(3) LogToConsole("Using Antiseptic") return true
elseif var[1]:find("Status: `4Awake!") and var[1]:find("command_2") then UseTools(2) LogToConsole("Using Anesthetic") return true
elseif var[1]:find("`4You can't see what you are doing(!+)") and var[1]:find("command_0") then UseTools(0) LogToConsole("Using Sponge") return true
elseif var[1]:find("`6It is becoming hard to see your work(.+)") and var[1]:find("command_0") then UseTools(0) LogToConsole("Using Sponge") return true
elseif var[1]:find("Patient is losing blood `4very quickly!") and var[1]:find("command_6") then UseTools(6) LogToConsole("Using Stitches") return true
elseif var[1]:find("Patient is losing blood `3slowly") and var[1]:find("command_6") then UseTools(6) LogToConsole("Using Stitches") return true
elseif var[1]:find("Patient is `6losing blood!") and var[1]:find("command_6") then UseTools(6) LogToConsole("Using Stitches") return true
elseif var[1]:find("command_7") and var[1]:find("command_6") then UseTools(6) LogToConsole("Using Stitches") return true
elseif var[1]:find("command_7") and var[1]:find("command_1") then UseTools(1) LogToConsole("Using Scalpel") return true
elseif var[1]:find("command_1") then UseTools(1) LogToConsole("Using Scalpel") return true
end
end
end

if DropApproved then
