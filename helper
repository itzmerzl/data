AddHook("OnSendPacket","Command",function (type, Command)

if Command:find("/menu") or Command:find("/proxy") or Command:find("action|friends") then Dialog_MainMenu() return true end
if Command:find("/social") then SendPacket(2,"action|friends") return true end
if Command:find("/lb") or Command:find("/leaderboard") then SendPacket(2,"action|dialog_return\ndialog_name|social\nbuttonClicked|leaderboard") return true end
if Command:find("/cmd") then local args = Divider(Command," ") if args[2] == nil then Dialog_ListCommand() return true elseif args[2] == "btk" then Dialog_BtkCommand() return true elseif args[2] == "csn" then Dialog_CsnCommand() return true elseif args[2] == "drop" then Dialog_DropCommand() return true end return true end
if Command:find("buttonClicked|CommandBtk") then Dialog_BtkCommand() return true end
if Command:find("buttonClicked|CommandDrop") then Dialog_DropCommand() return true end
if Command:find("buttonClicked|CommandAbility") then Dialog_AbilityCommand() return true end

if Command:find("/hostermenu") or Command:find("/hoster") then Dialog_HosterMenu() return true end

if Command:find("/log") or Command:find("/activitylog") then Dialog_ActivityLog() return true end
if Command:find("ShowAllCollectToggle|1\n") and not ShowAllCollect then if ShowAllCollect then return end ShowAllCollect = true LogToConsole("Capture log for all collected item `2Enabled") end 
if Command:find("ShowAllCollectToggle|0\n") and ShowAllCollect then if not ShowAllCollect then return end ShowAllCollect = false LogToConsole("Capture log for all collected item `4Disabled") end 
if Command:find("\nShowAllDropToggle|1") and not ShowAllDrop then if ShowAllDrop then return end ShowAllDrop = true LogToConsole("Capture log for all dropped item `2Enabled") end 
if Command:find("\nShowAllDropToggle|0") and ShowAllDrop then if not ShowAllDrop then return end ShowAllDrop = false LogToConsole("Capture log for all dropped item `4Disabled") end 

if Command:find("/actwrench") then WrenchLog() return true end 
if Command:find("dialog_name|ActivityWrenchLog\nbuttonClicked|") then WrenchType = Command:match("dialog_name|ActivityWrenchLog\nbuttonClicked|(%w+)\n") FilterWrench(WrenchType) return true end
if Command:find("/actdrop") then DropLog() return true end
if Command:find("/acttrash") then TrashLog() return true end
if Command:find("/actcollect") then CollectLog() return true end
if Command:find("/actenter") then EnterLog() return true end
if Command:find("/actexit") then ExitLog() return true end

if Command:find("/playerspin") or Command:find("/player") then Dialog_PlayerFilter() return true end
if Command:find("/p1") then if Player1Name == nil or Player1Name == "" then Bubble("Player 1 haven't set yet!") return true else SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..GetNetid(Player1Name).."|\nbuttonClicked|pull") Bubble("`wPulling P1 "..Player1Name) end return true end
if Command:find("/p2") then if Player2Name == nil or Player2Name == "" then Bubble("Player 2 haven't set yet!") return true else SendPacket(2,"action|dialog_return\ndialog_name|popup\nnetID|"..GetNetid(Player2Name).."|\nbuttonClicked|pull") Bubble("`wPulling P2 "..Player2Name) end return true end

if Command:find("/cctv") or Command:find("/spinlog") then SpinLog() return true end
if Command:find("dialog_name|WorldSpin\nbuttonClicked|(%d+)") then NetID = tonumber(Command:match("dialog_name|WorldSpin\nbuttonClicked|(%d+)")) FilterSpin(NetID) return true end

if Command:find("/taxeditor") or Command:find("/settax") then Dialog_TaxOptions() return true end
if Command:find("/count") then local Num1 = Command:match("/count (%d+)") local Num2 = Command:match("/count "..Num1.." (%d+)") Result = Num1*Num2 LogToConsole("Result : "..Config(Result)) return true end 

if Command:find("/ability") then Dialog_YourAbility() return true end
if Command:find("/join") or Command:find("/jm") then Dialog_AutoJoin() return true end
if Command:find("/autojoin") then if not AutoJoin then AutoJoin = true LogToConsole("Auto Join `2Enabled") else AutoJoin = false LogToConsole("Auto Join `4Disabled") end return true end
if Command:find("buttonClicked|JoinMessage") then if JoinMode ~= "Message" then JoinMode = "Message" Dialog_AutoJoin() else JoinMode = "" Dialog_AutoJoin() end return true end
if Command:find("buttonClicked|JoinPull") then if JoinMode ~= "Pull" then JoinMode = "Pull" Dialog_AutoJoin() else JoinMode = "" Dialog_AutoJoin() end return true end
if Command:find("buttonClicked|JoinKick") then if JoinMode ~= "Kick" then JoinMode = "Kick" Dialog_AutoJoin() else JoinMode = "" Dialog_AutoJoin() end return true end
if Command:find("buttonClicked|JoinBan") then if JoinMode ~= "Ban" then JoinMode = "Ban" Dialog_AutoJoin() else JoinMode = "" Dialog_AutoJoin() end return true end
if Command:find("AutoJoinToggle|1") then if AutoJoin then return end AutoJoin = true LogToConsole("Auto Join `2Enabled") return true end
if Command:find("AutoJoinToggle|0") then if not AutoJoin then return end AutoJoin = false LogToConsole("Auto Join `4Disabled") return true end
if Command:find("InputAutoMessage|") and not Command:find(AutoMessage) then AutoMessage = string.sub(string.sub(Command, 0, string.len(Command) - 1), Command:find("InputAutoMessage|") + 17) LogToConsole("Auto Message set to : "..AutoMessage) return true end


if Command:find("/skinoptions") then Dialog_SkinOptions() return true end
if Command:find("/skincustom") then Dialog_CustomSkin() return true end
if Command:find("/skinhitam") then SendPacket(2,"action|dialog_return\ndialog_name|skinpicker\nred|0\ngreen|0\nblue|0\ntransparency|0") return true end
if Command:find("/skinblack") then SendPacket(2,"action|setSkin\ncolor|1348237567") Overlay("Changed skin to `bBlack") return true end
if Command:find("/skinwhite") then SendPacket(2,"action|setSkin\ncolor|4042322175") Overlay("Changed skin to `wWhite") return true end
if Command:find("/skincream") then SendPacket(2,"action|setSkin\ncolor|3370516479") Overlay("Changed skin to `$Cream") return true end

if Command:find("/gscan") then Dialog_Growscan() return true end
if Command:find("buttonClicked|ScanThisFloat_") then ArrowScan(tonumber(Command:match("buttonClicked|ScanThisFloat_(%d+)")), true) return true end
if Command:find("buttonClicked|ScanThisBlock_") then ArrowScan(tonumber(Command:match("buttonClicked|ScanThisBlock_(%d+)")), false) return true end
if Command:find("buttonClicked|ScanWorldBlocks") then GrowscanBlock = true return true end
if Command:find("buttonClicked|ScanFloatingItems") then GrowscanFloat = true return true end

if Command:find("/service") or Command:find("/jasa") then Dialog_ServiceMenu() return true end

function FinderInAction() if math.floor(FinderItem) >= 14184 then Item = 14184 - FinderItem else Item = FinderItem end

if ItemFinderType == "Drop" then 
if GetAmount(FinderItem) <= 0 then SendPacket(2,"action|dialog_return\ndialog_name|item_search\n"..Item.."|1") Sleep(FinderDelay) end Drop(Item, GetAmount(math.floor(FinderItem))) Sleep(FinderDelay)
end

if ItemFinderType == "Magplant" then 
if GetAmount(FinderItem) <= 0 then SendPacket(2,"action|dialog_return\ndialog_name|item_search\n"..Item.."|1") Sleep(FinderDelay) end SendPacket(2,"action|dialog_return\ndialog_name|magplant_edit\nx|"..FindMagx.."|\ny|"..FindMagy.."|\nbuttonClicked|additems") Sleep(FinderDelay)
end

if ItemFinderType == "Vending" then 
if GetAmount(FinderItem) <= 0 then SendPacket(2,"action|dialog_return\ndialog_name|item_search\n"..Item.."|1") Sleep(FinderDelay) end SendPacket(2,"action|dialog_return\ndialog_name|vend_edit\nx|"..FindVendx.."|\ny|"..FindVendy.."|\nbuttonClicked|addstock") Sleep(FinderDelay)
end

end

Fi = "`7[`pFINDER`7]`o "
if Command:find("/if") then Dialog_ItemFinder() return true end
if Command:find("/sfind") then if ItemFinderType == "" then LogToConsole(Oops.."Looks like you haven't set the item finder yet!") return true elseif not ItemFinder then ItemFinder = true LogToConsole(Fi.."Item Finder `2Enabled") return true else ItemFinder = false LogToConsole(Fi.."Item Finder `4Disabled") end return true end 
if Command:find("buttonClicked|FinderDrop") then if ItemFinderType ~= "Drop" then ItemFinderType = "Drop" LogToConsole(Fi.."Item finder has been set to `1"..ItemFinderType) Dialog_ItemFinder() return true else ItemFinderType = "" LogToConsole(Fi.."Item finder has been reset") Dialog_ItemFinder() end return true end
if Command:find("buttonClicked|FinderMagplant") then if ItemFinderType ~= "Magplant" then ItemFinderType = "Magplant" LogToConsole(Fi.."Item finder has been set to `1"..ItemFinderType) FinderMagplant = true PunchConfig = true Bubble("Punch the Magplant to set the position") return true else ItemFinderType = "" LogToConsole(Fi.."Item finder has been reset") Dialog_ItemFinder() end return true end
if Command:find("buttonClicked|FinderVending") then if ItemFinderType ~= "Vending" then ItemFinderType = "Vending" LogToConsole(Fi.."Item finder has been set to `1"..ItemFinderType) FinderVending = true PunchConfig = true Bubble("Punch the Vending to set the position") return true else ItemFinderType = "" LogToConsole(Fi.."Item finder has been reset") Dialog_ItemFinder() end return true end
if Command:find("InputFinderItem|") and not Command:find("InputFinderItem|"..FinderItem) then FinderItem = string.sub(string.sub(Command, 0, string.len(Command) - 1), Command:find("InputFinderItem|") + 16) LogToConsole(Fi.."Finder Item set to : `w"..GetItemInfo(math.floor(FinderItem)).name) return true end 
if Command:find("InputFinderDelay|") and not Command:find("InputFinderDelay|"..FinderDelay) then FinderDelay = string.sub(string.sub(Command, 0, string.len(Command) - 1), Command:find("InputFinderDelay|") + 17) LogToConsole(Fi.."Finder delay set to `5"..FinderDelay) end

if Command:find("/cf") then Dialog_ClearFloat() return true end

Sb = "`7[`3BROADCAST`7]`o "
if Command:find("/sbpage") or Command:find("/setsb") or Command:find("/superb") then Dialog_SuperBroadcastEditor() return true end
if Command:find("/sbtext") then BroadcastText = Command:match("/sbtext (.+)") if BroadcastText == nil or BroadcastText == "" then LogToConsole(Sb.."You must enter the broadcast text correctly") return true end LogToConsole(Sb.."`6Broadacst text set to >> `o"..BroadcastText) return true end
if Command:find("buttonClicked|TestWebhook") then TestWebhookSB = true LogToConsole(Sb.."Webhook sent successful! please check on our discord. Run command `5/discord") return true end
if Command:find("InputCurrentBroadcast|") and not Command:find("InputCurrentBroadcast|"..CurrentBroadcast) then CurrentBroadcast = string.sub(Command, Command:find("InputCurrentBroadcast|") + 22, Command:find("InputCounterBroadcast|") - 2) LogToConsole(Sb.."Current Broadcast set to "..CurrentBroadcast) end 
if Command:find("InputCounterBroadcast|") and not Command:find("InputCounterBroadcast|"..CounterBroadcast) then CounterBroadcast = string.sub(Command, Command:find("InputCounterBroadcast|") + 22, Command:find("InputBroadcastTime|") - 2) LogToConsole(Sb.."Counter Broadcast set to "..CounterBroadcast) end
if Command:find("InputBroadcastTime|") and not Command:find("InputBroadcastTime|"..BroadcastTime) then BroadcastTime = string.sub(Command, Command:find("InputBroadcastTime|") + 19, Command:find("InputBroadcastText|") - 2) LogToConsole(Sb.."Broadcast Time set to "..BroadcastTime.." minutes") end 
if Command:find("InputBroadcastText|") and not Command:find("InputBroadcastText|"..BroadcastText) then BroadcastText = string.sub(Command, Command:find("InputBroadcastText|") + 19, Command:find("InputBroadcastDiscordID|") - 2) LogToConsole(Sb.."Broadcast Text set to "..BroadcastText) end 
--if Command:find("InputBroadcastWebhook|") and not Command:find("InputBroadcastWebhook|"..BroadcastWebhookLink) then BroadcastWebhookLink = string.sub(Command, Command:find("InputBroadcastWebhook|") + 22, Command:find("InputBroadcastDiscordID|") - 2) LogToConsole("Broadcast webhook set to "..BroadcastWebhookLink) end 
if Command:find("InputBroadcastDiscordID|") and not Command:find("InputBroadcastDiscordID|"..BroadcastDiscordID) then BroadcastDiscordID = string.sub(Command, Command:find("InputBroadcastDiscordID|") + 24, Command:find("InputBroadcastWorld|") - 2) LogToConsole(Sb.."Discord user id set to "..BroadcastDiscordID) end 
if Command:find("InputBroadcastWorld|") and not Command:find("InputBroadcastWorld|"..BroadcastWorld) then BroadcastWorld = string.sub(string.sub(Command, 0, string.len(Command) - 1), Command:find("InputBroadcastWorld|") + 20) LogToConsole(Sb.."Broadcast World set to "..BroadcastWorld) end 

if Command:find("buttonClicked|CounterToUp") then if CounterMode ~= "Up" then CounterMode = "Up" LogToConsole(Sb.."Count mode set to : `3"..CounterMode) Dialog_SuperBroadcastEditor() else LogToConsole(Oops.."`oLooks like counter mode already set to `6"..CounterMode) end end
if Command:find("buttonClicked|CounterToDown") then if CounterMode ~= "Down" then CounterMode = "Down" LogToConsole(Sb.."Count mode set to : `3"..CounterMode) Dialog_SuperBroadcastEditor() else LogToConsole(Oops.."`oLooks like counter mode already set to `6"..CounterMode) end end
if Command:find("buttonClicked|CounterModeSB") then if StartingSuperBroadcast then LogToConsole(Oops.."You're on broadcast, you can't change the data!") return true elseif BroadcastMode ~= "Counter" then BroadcastMode = "Counter" LogToConsole(Sb.."Broadcast mode set to `6"..BroadcastMode) Dialog_SuperBroadcastEditor() else LogToConsole(Oops.."`oLooks like broadcast mode already set to `6"..BroadcastMode) end end
if Command:find("buttonClicked|TimerModeSB") then if StartingSuperBroadcast then LogToConsole(Oops.."You're on broadcast, you can't change the data!") return true elseif BroadcastMode ~= "Timer" then BroadcastMode = "Timer" LogToConsole(Sb.."Broadcast mode set to `6"..BroadcastMode) Dialog_SuperBroadcastEditor() else LogToConsole(Oops.."`oLooks like broadcast mode already set to `6"..BroadcastMode) end end
if Command:find("buttonClicked|ResetDataSB") then if StartingSuperBroadcast then LogToConsole(Oops.."You're on broadcast, you can't change the data!") return true end CounterBroadcast, CurrentBroadcast, BroadcastGemsUsed, BroadcastText, BroadcastTime, MentionUser, BroadcastWebhook, BroadcastWebhookLink, BroadcastDiscordID = 0, 0, 0, "Starries Helper by `5@starries.real", 60, false, false, "https://discord.com/api/webhooks/1201423910395191336/yIxpvnODZcC166_ArLfaJnLF4wY48IqB6uW32FfQgwIH39GU0dTveGyVDvKLyIyY3NtW", 0 Overlay(Credit.."`wResetting super broadcast data successful!") Dialog_SuperBroadcastEditor() end
if Command:find("buttonClicked|SendSuperBroadcast") or Command:find("/ssb") then if BroadcastMode == "" then LogToConsole(Oops.."You haven't set the mode for broadcast") return true elseif CounterBroadcast == 0 and BroadcastMode == "Counter" then LogToConsole(Oops.."You haven't set amount of counter broadcast, needed when you use Counter mode") return true end if not StartingSuperBroadcast then TimeStart = os.time() StartingSuperBroadcast = true AlreadyBroadcast = false Overlay(Credit.."`wBroadcasting has been `2Enabled") return true else BroadcastGemsUsed, CurrentBroadcast = 0, 0 StartingSuperBroadcast = false AlreadyBroadcast = true Overlay(Credit.."`wBroadcasting has been `4Disabled") end return true end
if Command:find("buttonClicked|MentionUserSB") then if StartingSuperBroadcast then LogToConsole(Oops.."You're on broadcast, you can't change the data!") return true elseif BroadcastDiscordID == 0 then LogToConsole(Oops.."Look like discord user id hasn't been configured yet, please set your discord id!") return true elseif not MentionUser then MentionUser = true LogToConsole(Sb.."Mention User on webhook `2Enabled") Dialog_MentionMenu() else MentionUser = false MentionMode = "" LogToConsole(Sb.."Mention User on webhook `4Disabled") Dialog_SuperBroadcastEditor() end end
if Command:find("/mention1") then if MentionMode ~= "All" then MentionMode = "All" LogToConsole(Sb.."Mention user mode set to "..MentionMode) Dialog_SuperBroadcastEditor() else MentionMode = "" LogToConsole(Sb.."Mention user mode has been reset") Dialog_SuperBroadcastEditor() end return true end
if Command:find("/mention2") then if MentionMode ~= "End" then MentionMode = "End" LogToConsole(Sb.."Mention user mode set to "..MentionMode) Dialog_SuperBroadcastEditor() else MentionMode = "" LogToConsole(Sb.."Mention user mode has been reset") Dialog_SuperBroadcastEditor() end return true end
if Command:find("buttonClicked|BackWorldSB") then if StartingSuperBroadcast then LogToConsole(Oops.."You're on broadcast, you can't change the data!") return true elseif BroadcastWorld == nil and BroadcastWorld == "" then LogToConsole(Oops.."Look like world back hasn't been configured yet, please set your world!") return true elseif not BackWorld then BackWorld = true LogToConsole(Sb.."Back world after broadcast done `2Enabled") Dialog_SuperBroadcastEditor() else BackWorld = false LogToConsole(Sb.."Back world after broadcast done `4Disabled") Dialog_SuperBroadcastEditor() end end
if Command:find("buttonClicked|GetSignText") then SignBroadcastText = true Bubble("Wrench the sign to get the text") return true end

if Command:find("Player1Pick|(%d+)") then NetID = tonumber(Command:match("Player1Pick|(%d+)")) for __, player in pairs(GetPlayerList()) do if player.netid == NetID then if player.userid == GetLocal().userid then Bubble("`wYou can't pick yourself, captain.") return true end if player.userid == Player2Uid then Bubble("User already locked to Player 2!") return true end Bubble("`wPlayer 1 Lock to : [ " ..player.name.." `w]") LogToConsole("`7[`rPlayer Filter`7] `oPlayer 1 Lock to : [ " ..player.name.." `o]") Player1Uid, Player1Name = player.userid, player.name return true end end end
if Command:find("Player2Pick|(%d+)") then NetID = tonumber(Command:match("Player2Pick|(%d+)")) for __, player in pairs(GetPlayerList()) do if player.netid == NetID then if player.userid == GetLocal().userid then Bubble("`wYou can't pick yourself, captain.") return true end if player.userid == Player1Uid then Bubble("User already locked to Player 1!") return true end Bubble("`wPlayer 2 Lock to : [ " ..player.name.." `w]") LogToConsole("`7[`rPlayer Filter`7] `oPlayer 2 Lock to : [ " ..player.name.." `o]") Player2Uid, Player2Name = player.userid, player.name return true end end end
if Command:find("/resetplayer1") then Player1Spin, Player1Name, Player1Uid = "Pick player 1 first to show his spin", "", 0 Overlay("Player 1 has been reset") return true end
if Command:find("/resetplayer2") then Player2Spin, Player2Name, Player2Uid = "Pick player 2 first to show his spin", "", 0 Overlay("Player 2 has been reset") return true end

if Command:find("/dp") or Command:find("/depo") or Command:find("/deposit") then local args = Divider(Command," ") if args[2] == nil then LogToConsole(Oops.."To deposit Blue Gem Lock to the Bank use this command `5/dp <amount>") return true end Amount = args[2] if Amount == nil then LogToConsole(Oops.."Amount of deposit should be a number!") elseif Amount == 0 then LogToConsole(Oops.."You can't deposit `w"..Amount.." `oBlue Gem Lock") else SendPacket(2,"action|dialog_return\ndialog_name|bank_deposit\nbgl_count|"..Amount) end return true end 
if Command:find("/wd") or Command:find("/draw") or Command:find("/withdraw") then local args = Divider(Command," ") if args[2] == nil then LogToConsole(Oops.."To withdrawn Blue Gem Lock from the Bank use this command `5/wd <amount>") return true end Amount = args[2] if Amount == nil then LogToConsole(Oops.."Amount of deposit should be a number!") elseif Amount == 0 then LogToConsole(Oops.."You can't withdraw `w"..Amount.." `oBlue Gem Lock") else SendPacket(2, "action|dialog_return\ndialog_name|bank_withdraw\nbgl_count|"..Amount) end return true end

