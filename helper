function Consumeable() if ConsumeArroz then if GetAmount(4604) <= 0 then LogToConsole(Oops.."You don't have "..GetItemInfo(4604).name.." in your inventory") return true end Place(4604, math.floor(GetLocal().pos.x/32), math.floor(GetLocal().pos.y/32)) Sleep(500) end if ConsumeEggs then if GetAmount(1474) <= 0 then LogToConsole(Oops.."You don't have "..GetItemInfo(1474).name.." in your inventory") return true end Place(1474, math.floor(GetLocal().pos.x/32), math.floor(GetLocal().pos.y/32)) Sleep(500) end if ConsumeClover then if GetAmount(528) <= 0 then LogToConsole(Oops.."You don't have "..GetItemInfo(528).name.." in your inventory") return true end Place(528, math.floor(GetLocal().pos.x/32), math.floor(GetLocal().pos.y/32)) Sleep(500) end if ConsumeSongpyeon then if GetAmount(1056) <= 0 then LogToConsole(Oops.."You don't have "..GetItemInfo(1056).name.." in your inventory") return true end Place(1056, math.floor(GetLocal().pos.x/32), math.floor(GetLocal().pos.y/32)) end end
if Command:find("/consume") or Command:find("/cm") then Dialog_ConsumeMenu() return true end
if Command:find("/autoconsume") then if AutoConsume then AutoConsume = false LogToConsole("Auto Consume `4Disabled") else AutoConsume = true LogToConsole("Auto Consume `2Enabled") ConsumeNow = true end return true end
if Command:find("buttonClicked|CsArroz") then if not ConsumeArroz then ConsumeArroz = true Dialog_ConsumeMenu() else ConsumeArroz = false Dialog_ConsumeMenu() end return true end
if Command:find("buttonClicked|CsEggs") then if not ConsumeEggs then ConsumeEggs = true Dialog_ConsumeMenu() else ConsumeEggs = false Dialog_ConsumeMenu() end return true end
if Command:find("buttonClicked|CsClover") then if not ConsumeClover then ConsumeClover = true Dialog_ConsumeMenu() else ConsumeClover = false Dialog_ConsumeMenu() end return true end
if Command:find("buttonClicked|CsSongpyeon") then if not ConsumeSongpyeon then ConsumeSongpyeon = true Dialog_ConsumeMenu() else ConsumeSongpyeon = false Dialog_ConsumeMenu() end return true end
if Command:find("buttonClicked|CsBgems") then if not ConsumeBlackGems then ConsumeBlackGems = true Dialog_ConsumeMenu() else ConsumeBlackGems = false Dialog_ConsumeMenu() end return true end
if Command:find("AutoConsumeToggle|1") then if AutoConsume then return end AutoConsume = true LogToConsole("Auto Consume `2Enabled") ConsumeNow = true return true end
if Command:find("AutoConsumeToggle|0") then if not AutoConsume then return end AutoConsume = false LogToConsole("Auto Consume `4Disabled") return true end

