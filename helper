function BtkAction() 
if Posx1 == 0 and Posx2 == 0 then LogToConsole(Oops.."Set Position 1 & Position 2 first with command `5/pos1-2") return true end 

if CheckBet then
DroptoPos1, DroptoPos2 = 0, 0 for _, objects in pairs(GetObjectList()) do if objects.pos.x//32 == Posx1 and objects.pos.y//32 == Posy1 then if objects.id == 242.0 then DroptoPos1 = DroptoPos1 + objects.amount end if objects.id == 1796.0 then DroptoPos1 = DroptoPos1 + objects.amount * 100 end if objects.id == 7188.0 then DroptoPos1 = DroptoPos1 + objects.amount * 10000 end if objects.id == 11550.0 then DroptoPos1 = DroptoPos1 + objects.amount * 1000000 end end if objects.pos.x//32 == Posx2 and objects.pos.y//32 == Posy2 then if objects.id == 242.0 then DroptoPos2 = DroptoPos2 + objects.amount end if objects.id == 1796.0 then DroptoPos2 = DroptoPos2 + objects.amount * 100 end if objects.id == 7188.0 then DroptoPos2 = DroptoPos2 + objects.amount * 10000 end if objects.id == 11550.0 then DroptoPos2 = DroptoPos2 + objects.amount * 1000000 end end end DroptoPos1, DroptoPos2 = math.floor(DroptoPos1), math.floor(DroptoPos2) CheckBet = false if DroptoPos1 == 0 or nil and DropToPos2 == 0 or nil then LogToConsole(Oops.."There's no bets can be taken yet!") Chat("There's no bets can be taken yet!") return true elseif DroptoPos1 ~= DroptoPos2 then LogToConsole(Oops.."The amount of bet are'nt equal, make sure to drop same bet!") Chat("The amount of bet are'nt equal, make sure to drop same bet!") return true else LogToConsole("P1 : `2"..Config(DroptoPos1).. " `oI P2 : `2"..Config(DroptoPos2).." `oI Tax : `2"..Tax1.."`o% I Drop : `2"..Config(CountTax(DroptoPos2 * 2, Tax1))) Overlay("Both bets are equal\n"..Tax1.."% Tax\nPlayer 1 : "..Config(DroptoPos1) .. "\n`wPlayer 2 : " .. Config(DroptoPos2) .. " \n`wAmount to drop : " .. Config(CountTax(DroptoPos2 * 2, Tax1))) Bet = DroptoPos1 Collect(Posx1-1, Posy1) Collect(Posx1, Posy1) Collect(Posx1+1, Posy1) Collect(Posx2-1, Posy2) Collect(Posx2, Posy2) Collect(Posx2+1, Posy2) Chat(AfterCollectBetBubbleText) end
end
Sleep(5000)

end

if Command:find("/autohost") then HostingBTK = true return true end
end

if Command:find("/bbet") then if Bet == 0 then LogToConsole("Can't detect amount bet, set amount bet with command `5/bet <amount>") return true end BackBetNow = true return true end
if Command:find("/givetax") or Command:find("/b2") then if Postx == 0 or Postx == nil then LogToConsole(Oops.."Set the position of donation box for tax") return true end Percentage = 50 SplittedWL = CountTax(GetAmount(242), Percentage) SplittedDL = CountTax(GetAmount(1796), Percentage) SplittedBGL = CountTax(GetAmount(7188), Percentage) SplittedIRENG = CountTax(GetAmount(11550), Percentage) GiveTheTax = true return true end
