function CheckPosition(x, y) local result if x == 0 and y == 0 then result = "Not set yet" else result = x .. ", " .. y end return result end

function Dialog_ConfigBTK() if Horizontal then PositionMode = "Horizontal Mode" elseif Vertical then PositionMode = "Vertical Mode" else PositionMode = "`5Not set yet" end if PutMode == "Remote" then frameRemote = "staticPurpleFrame" else frameRemote = "staticGreyFrame" end if PutMode == "Chand" then frameChand = "staticPurpleFrame" else frameChand = "staticGreyFrame" end if not Horizontal and not Vertical then PC1 = {} table.insert(PC1, "Not set yet") PC2 = {} table.insert(PC2, "Not set yet") elseif Horizontal then PC1 = {} table.insert(PC1, CheckPosition(Posxgem2, Posygem1)..", "..CheckPosition(Posxgem1, Posygem1)..", "..CheckPosition(Posxgem3, Posygem1)) PC2 = {} table.insert(PC2, CheckPosition(PPosxgem2, PPosygem1)..", "..CheckPosition(PPosxgem1, PPosygem1)..", "..CheckPosition(PPosxgem3, PPosygem1)) elseif Vertical then PC1 = {} table.insert(PC1, CheckPosition(Posxgem1, Posygem1)..", "..CheckPosition(Posxgem1, Posygem2)..", "..CheckPosition(Posxgem1, Posygem3)) PC2 = {} table.insert(PC2, CheckPosition(PPosxgem1, PPosygem1)..", "..CheckPosition(PPosxgem1, PPosygem2)..", "..CheckPosition(PPosxgem1, PPosygem3)) end C = [[
set_default_color|`]]..PrimaryColor..[[|
add_label_with_icon|big|Configuration|left|4630|
add_spacer|small|
add_label|small|Position Bet :|left|
add_smalltext|[LEFT] `]]..SecondaryColor..[[]]..CheckPosition(Posx1, Posy1)..[[ `]]..PrimaryColor..[[and `]]..SecondaryColor..[[]]..CheckPosition(Posx2, Posy2)..[[ `]]..PrimaryColor..[[[RIGHT]|
